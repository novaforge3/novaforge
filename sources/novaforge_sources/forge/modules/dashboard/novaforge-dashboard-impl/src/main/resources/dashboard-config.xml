<dashboard-config>
	<layouts>
		<layout key="one_horizontal" row="1" column="1">
			<area id="1">
				<top-left row="1" column="1" />
				<bottom-right row="1" column="1" />
			</area>
		</layout>
		<layout key="two_horizontal" row="1" column="2">
			<area id="1">
				<top-left row="1" column="1" />
				<bottom-right row="1" column="1" />
			</area>
			<area id="2">
				<top-left row="1" column="2" />
				<bottom-right row="1" column="2" />
			</area>
		</layout>
		<layout key="two_vertical" row="2" column="1">
			<area id="1">
				<top-left row="1" column="1" />
				<bottom-right row="1" column="1" />
			</area>
			<area id="2">
				<top-left row="2" column="1" />
				<bottom-right row="2" column="1" />
			</area>
		</layout>
		<layout key="three_horizontal" row="1" column="3">
			<area id="1">
				<top-left row="1" column="1" />
				<bottom-right row="1" column="1" />
			</area>
			<area id="2">
				<top-left row="1" column="2" />
				<bottom-right row="1" column="2" />
			</area>
			<area id="3">
				<top-left row="1" column="3" />
				<bottom-right row="1" column="3" />
			</area>
		</layout>
		<layout key="three_vertical" row="3" column="1">
			<area id="1">
				<top-left row="1" column="1" />
				<bottom-right row="1" column="1" />
			</area>
			<area id="2">
				<top-left row="2" column="1" />
				<bottom-right row="2" column="1" />
			</area>
			<area id="3">
				<top-left row="3" column="1" />
				<bottom-right row="3" column="1" />
			</area>
		</layout>
		<layout key="one_vertical_two_vertical" row="2" column="2">
			<area id="1">
				<top-left row="1" column="1" />
				<bottom-right row="2" column="1" />
			</area>
			<area id="2">
				<top-left row="1" column="2" />
				<bottom-right row="1" column="2" />
			</area>
			<area id="3">
				<top-left row="2" column="2" />
				<bottom-right row="2" column="2" />
			</area>
		</layout>
		<layout key="two_vertical_one_vertical" row="2" column="2">
			<area id="1">
				<top-left row="1" column="1" />
				<bottom-right row="1" column="1" />
			</area>
			<area id="2">
				<top-left row="2" column="1" />
				<bottom-right row="2" column="1" />
			</area>
			<area id="3">
				<top-left row="1" column="2" />
				<bottom-right row="2" column="2" />
			</area>
		</layout>
		<layout key="two_horizontal_one_horizontal" row="2" column="2">
			<area id="1">
				<top-left row="1" column="1" />
				<bottom-right row="1" column="1" />
			</area>
			<area id="2">
				<top-left row="1" column="2" />
				<bottom-right row="1" column="2" />
			</area>
			<area id="3">
				<top-left row="2" column="1" />
				<bottom-right row="2" column="2" />
			</area>
		</layout>
		<layout key="one_horizontal_two_horizontal" row="2" column="2">
			<area id="1">
				<top-left row="1" column="1" />
				<bottom-right row="1" column="2" />
			</area>
			<area id="2">
				<top-left row="2" column="1" />
				<bottom-right row="2" column="1" />
			</area>
			<area id="3">
				<top-left row="2" column="2" />
				<bottom-right row="2" column="2" />
			</area>
		</layout>
	</layouts>
</dashboard-config>
