<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:common="urn:import:org.novaforge.forge.tools.deliverymanager.ui.client.view.commons.celltree.nodesselection"
             xmlns="urn:import:org.novaforge.forge.tools.deliverymanager.ui.client">
    <ui:with field='res'
             type='org.novaforge.forge.ui.commons.client.resources.CommonsResources'/>
    <g:ScrollPanel alwaysShowScrollBars="false">
        <g:VerticalPanel spacing="5" width='100%' verticalAlignment="ALIGN_TOP">
            <g:Label ui:field='deliveryTitleLabel' addStyleNames="{res.css.zoneTitle}"/>
            <common:NodesSelectionTrees ui:field="filesSelectionTrees"/>
            <g:HorizontalPanel spacing="5" horizontalAlignment="ALIGN_CENTER" width='100%'>
                <g:HorizontalPanel spacing="5" horizontalAlignment="ALIGN_CENTER">
                    <g:Button addStyleNames="{res.css.important}" ui:field="buttonSaveDocumentsDelivery"/>
                </g:HorizontalPanel>
            </g:HorizontalPanel>
        </g:VerticalPanel>
    </g:ScrollPanel>
</ui:UiBinder> 