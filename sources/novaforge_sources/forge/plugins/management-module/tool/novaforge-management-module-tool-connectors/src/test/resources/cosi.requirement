<?xml version="1.0" encoding="UTF-8"?>
<requirement:Repository xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:requirement="http://www.obeonetwork.org/dsl/requirement/1.0" xmi:id="XMI_ID" name="COSI">
<mainCategories xmi:id="_44MKQWmeEeCS0b_N7fJ90g" name="Administrer COSI" id="M02">
    <subCategories xmi:id="_44MKQmmeEeCS0b_N7fJ90g" name="Administrer les données de référence" id="CU02">
      <subCategories xmi:id="_44MKQ2meEeCS0b_N7fJ90g" name="CRUD projet" id="CU02_01">
        <requirements xmi:id="_44MKRGmeEeCS0b_N7fJ90g" name="Projet - Générer le code projet - A REVOIR " id="CS_RGF_02_001" statement="Le code projet est constitué automatiquement par une concaténation des attributs saisis suivants: A REVOIR  code attributaire de l'opé  c'est à dire soit la base de défense de l'opé soit autorité d'emploi de l'opé (créer dans la tâche &quot;renseigner le qui / ou&quot;) + année de création de l'opé (créer dans la tâche &quot;renseigner identification de l'opération&quot;) +compteur (4 chiffres mini)&#xD;&#xA;&#xD;&#xA;séparer par &quot;_&quot;" rationale="CS_FCT_010275001" acceptanceCriteria=""/>
        <requirements xmi:id="_44MKRWmeEeCS0b_N7fJ90g" name="Renseigner l'état du projet par défaut" id="CS_RGF_02_002" statement="Par défaut le projet a pour état &quot;en cours&quot;" rationale="CS_FCT_010275002" acceptanceCriteria=""/>
        <requirements xmi:id="_44MKRmmeEeCS0b_N7fJ90g" name="Montant enveloppe du projet positif A REVOIR" id="CS_RGF_02_003" statement="Le montant de l'enveloppe projet doit être positif" rationale="CS_FCT_010275003" acceptanceCriteria=""/>
      </subCategories>
      <subCategories xmi:id="_ugOsUIVHEeCLO4soIxPKpg" name="Gérer les modèles de planning" id="CU02_02">
        <requirements xmi:id="_ckS1cFO_EeGQtK6mgdcOyg" name="Droits : accès à gérer les modèles de planning" id="CS_RGF_02_02_150" statement="Seuls les utilisateurs ayant un profil avec la fonction « consulter les phases et les modèles de planning »  peuvent accéder à cette tâche">
          
          
        </requirements>
        <requirements xmi:id="_5ZZrYIdiEeCeiKVfaqHkig" name="Modèle Planning - Règle de présentation de la liste des modèles de planning" id="CS_RGF_02_120" statement="Le système doit présenter la liste des modèles de planning (code, libellé abrégé, libellé, type Moe, nature phase) en choix simple triée par ordre croissant du code du modèle de planning, puis du libellé abrégé puis du libellé du modèle.&#xD;&#xA;Il doit présenter aussi l’obsolescence des modèles." rationale="">
          
          
        </requirements>
        <requirements xmi:id="__8TVYKiYEeCM-5KAINOWpw" name="Modèle Planning - Règle de pr��sentation du commentaire" id="CS_RGF_02_207" statement="Le système doit présenter un tooltip (au passage de la souris sur chaque ligne de la liste de modèles) qui présente le commentaire du modèle de planning de la liste :&#xD;&#xA;&quot;Commentaire :&quot; + Commentaire du modèle de planning." rationale="">
          
          
        </requirements>
        <requirements xmi:id="_Wt6uIKlsEeCu6-1RnGqdag" name="Phase - Règle de présentation des modèles de planning obsolètes" id="CS_RGF_02_213" statement="Les modèles de planning obsolètes sont présentés avec le texte en rouge sur demande dans l'arborescence des modèles planning et ne pas les rendre ni modifiable ni supprimable. &#xD;&#xA;&#xD;&#xA;" rationale="">
          
          
          
          
          
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_EqXMMIVIEeCLO4soIxPKpg" name="Modifier un modèle de planning" id="CU02_04">
        <requirements xmi:id="_xGAmAFO_EeGQtK6mgdcOyg" name="Droits : accès à modifier les modèles de planning" id="CS_RGF_02_02_450" statement="Seuls les utilisateurs ayant un profil avec la fonction « créer, modifier, supprimer un modèle de planning  »  peuvent accéder à cette tâche">
          
          
          
          
          
          
        </requirements>
        <requirements xmi:id="_yh8YgFO_EeGQtK6mgdcOyg" name="Droits : accès à consulter les modèles de planning" id="CS_RGF_02_02_451" statement="Les utilisateurs ayant un profil avec la fonction « consulter les phases et les modèles de planning »  peuvent également accéder à cette tâche mais en lecture seule.">
          
          
          
          
        </requirements>
        <requirements xmi:id="_YNoGMIdmEeCeiKVfaqHkig" name="création modèle de planning - liste choix nature d'opération" id="CS_RGF_02_121" statement="Nature d'opération (RF_NAT)&#xD;&#xA;Sélection simple&#xD;&#xA;actif (xtopsup=0)&#xD;&#xA;Résultat :&#xD;&#xA;- trié par libellé croissant&#xD;&#xA;Champs affichés :&#xD;&#xA;- RF_NAT_LIB,&#xD;&#xA;Champs retournés :&#xD;&#xA;- RF_NAT_LIB,">
          
          
        </requirements>
        <requirements xmi:id="_5ex90IdmEeCeiKVfaqHkig" name="Modèle Planning - création modèle de planning" id="CS_RGF_02_122" statement="Le système doit vérifier qu'aucun autre modèle de planning (non obsolète) ne porte le même libellé ou / et libellé abrégé (sans tenir compte de la casse) et / ou code ou sortir en erreur et sinon enregistrer le nouveau modèle de planning." rationale="">
          
          
        </requirements>
        <requirements xmi:id="_5ex90YdmEeCeiKVfaqHkig" name="Modèle Planning - Msg erreur création modèle de planning ?" id="CS_RGF_02_123" statement="Si le système ne peut créer de modèle de planning pour raison du libellé et / ou libellé abrégé et / ou code existant, il doit proposer le message suivant dans une boîte de dialogue &quot;OK&quot; :&#xD;&#xA;&quot;Création impossible, un autre modèle de planning porte déjà ce libellé (ou ce code) (ou ce libellé abrégé)  (ou ce libellé et ce code) (ou ce libellé abrégé et ce code) (ou ce libellé et ce libellé abrégé) (ou ce libellé, ce libéllé abrégé et ce code), veuillez le(s) modifier&quot;&#xD;&#xA;Il y a donc 7 messages possibles. ">
          
          
          
        </requirements>
        <requirements xmi:id="_HLercId0EeCeiKVfaqHkig" name="Modèle Planning - enregistrement liste des phases" id="CS_RGF_02_126" statement="Le système doit enregistrer la liste des phases depuis celles du catalogue en lui renseignant le rang de la phase, son identifiant et sa date de dernière modification dans le modèle de planning et en convertissant les mois en jour (30 jours dans un mois) ">
          
        </requirements>
        <requirements xmi:id="_aiT0MJJfEeC4A-726iExUQ" name="Modèle Planning - modification durée / unité de durée phase" id="CS_RGF_02_150" statement="Le système doit enregistrer pour chacune des phases liées une durée positive qui est par défaut &quot;0&quot;.">
          
          
          
        </requirements>
        <requirements xmi:id="_JfpK0JipEeC2N5fmUmQWNQ" name="Modèle Planning - consultation" id="CS_RGF_02_151" statement="Si une consultation est demandée, l'écran de modification de phase est en lecture seule.">
          
          
        </requirements>
        <requirements xmi:id="_bMJzYJiqEeC2N5fmUmQWNQ" name="Modèle Planning - condition minimale de validation" id="CS_RGF_02_152" statement="La validation  de la création / modification d'un modèle de planning n'est possible et disponible à l'utilisateur que si le libellé abrégé, le libellé et le code de la phase sont renseignés et non vides. Il ne doit pas y avoir plusieurs phases avec un libellé abrégé ou un libellé  identique.">
          
          
        </requirements>
        <requirements xmi:id="_TmSjUJisEeC2N5fmUmQWNQ" name="Modèle de planning - initialisation de l'écran de modification" id="CS_RGF_02_153" statement="Le système doit récupérer l'ensemble des informations du modèle de planning, les référentiels de type de MOE (REF_CAL) et nature d'opération (REF_OP_PRJ) puis mettre à jour l'écran de modification de phase avec les valeurs récupérées.">
          
          
          
          
        </requirements>
        <requirements xmi:id="_KKAZoJitEeC2N5fmUmQWNQ" name="Modèle Planning - modification modèle de planning" id="CS_RGF_02_154" statement="Le système doit vérifier qu'aucun autre modèle de planning ne porte le même libellé ou / et libellé abrégé et / ou code ou sortir en erreur et sinon enregistrer le modèle de planning." rationale=""/>
        <requirements xmi:id="_XcPrwJitEeC2N5fmUmQWNQ" name="Modèle Planning - Msg erreur modification modèle de planning ?" id="CS_RGF_02_155" statement="Si le système ne peut modifier de modèle de planning pour raison du libellé et / ou libellé abrégé et / ou code existant, il doit proposer le message suivant dans une boîte de dialogue &quot;OK&quot; :&#xD;&#xA;&quot;Modification impossible, un autre modèle de planning porte déjà ce libellé (ou ce code) (ou ce libellé abrégé)  (ou ce libellé et ce code) (ou ce libellé abrégé et ce code) (ou ce libellé et ce libellé abrégé) (ou ce libellé, ce libellé abrégé et ce code), veuillez le(s) modifier&quot;&#xD;&#xA;Il y a donc 7 messages possibles. "/>
        <requirements xmi:id="_Icst0JiuEeC2N5fmUmQWNQ" name="Modèle de planning - initialisation de l'écran de création" id="CS_RGF_02_156" statement="Le système doit récupérer les référentiels de type de MOE (REF_CAL) et nature d'opération  (REF_OP_PRJ) puis mettre à jour l'écran de création de modèle de planning avec les valeurs récupérées." acceptanceCriteria="">
          
          
        </requirements>
        <requirements xmi:id="_WcO4sId0EeCeiKVfaqHkig" name="Modèle Planning - tri liste de phase" id="CS_RGF_02_162" statement="Le système doit permettre de filtrer la liste des phases par chacune des colonnes présentes." rationale="" acceptanceCriteria="">
          
          
        </requirements>
        <requirements xmi:id="_xw9XYKigEeCM-5KAINOWpw" name="Modèle de planning - règle de présentation des informations de jalon" id="CS_RGF_02_208" statement="Le système doit présenter un tooltip (au passage de la souris sur chaque ligne de la liste des phases du modèle) qui présente les libellés de début et fin du jalon associé à la phase du modèle de planning de la liste :&#xD;&#xA;&quot;Jalon : &quot; &#xD;&#xA;+ libellé jalon début de la phase du modèle de planning,&#xD;&#xA;+ libellé jalon fin de la phase du modèle de planning.">
          
          
          
          
        </requirements>
        <requirements xmi:id="_TuGyUKihEeCM-5KAINOWpw" name="Modèle de planning - règle de présentation de la date de dernière modification de la phase initiale" id="CS_RGF_02_209" statement="Le système doit présenter la date de dernière modification de la phase du modèle de planning de la liste :&#xD;&#xA;&quot;Date de dernière modification : &quot; + dateDerniereModification de la phase du modèle de planning.&#xD;&#xA;" rationale="">
          
          
          
          
          
          
        </requirements>
        <requirements xmi:id="_rFBuAKloEeCu6-1RnGqdag" name="Modèle Planning - Règle de présentation des phases dont le modèle est devenu obsolète" id="CS_RGF_02_210" statement="Le système doit présenter une phase du modèle dont le modèle est devenu &quot;obsolète&quot; en rouge (pour le texte)." rationale="">
          
          
        </requirements>
        <requirements xmi:id="_X2Qv4LzsEeCd96ABgxeEzQ" name="Modèle de planning – initialisation de la date de création" id="CS_RGF_02_234" statement="Lors de la création du modèle de planning, la date de création du modèle est initialisée à la date du jour." rationale="">
          
        </requirements>
        <requirements xmi:id="__DsOoN3bEeCe1-wC7er0Ow" name="Modification de l'unité de durée" id="CS_RGF_02_307" statement="La modification de l'unité de durée provoque la mise à jour des durées pour chaque phase selon le calcul simplifié suivant :&#xD;&#xA;- 1 mois = 30 jours&#xD;&#xA;L’arrondi se fait au nombre le plus proche. Deux chiffres après la virgule pour les mois, zéro pour les jours.&#xD;&#xA;&#xD;&#xA;Cela provoque aussi la modification de l'indication d'unité affichée après les valeurs :&#xD;&#xA;- m (pour mois)&#xD;&#xA;- j (pour jour)&#xD;&#xA;&#xD;&#xA;exemple : 4m&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="__DsOod3bEeCe1-wC7er0Ow" name="Saisie de durée de phase" id="CS_RGF_02_308" statement="La durée est saisie selon l'unité sélectionnée (jour, ou mois). &#xD;&#xA; - entier si l'unité est jour &#xD;&#xA; - avec deux décimales si mois&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_WujlkDtfEeGw_pr_unSqQw" name="Choisir organisme niveau 1 ou 2" id="CS_RGF_02_309" statement="Il existe des catalogues de modèles de planning pour chaque ESID et chaque DID&#xD;&#xA;&#xD;&#xA;A la création / modification d'un modèle de planning, l'utilisateur doit choisir un organisme pour déterminer si le modèle s'applique pour 1 organisme de niveau 2 : 1 ESID particulier ou 1 DID particulière (choix du code organisme de l'ESID ou de la DID) ou pour tous les ESID et DID (choix du code organisme du SID) " status="">
          
          
        </requirements>
        <requirements xmi:id="_L8IrMFizEeGDiYDSMh9IIA" name="Consulter un modele de planning - switch jours - mois" id="CS_RGF_02_310" statement="En consultation d'un modèle de planning, les choix d'affichages en jour ou mois des durées de phases doivent être disponibles à l'utilisateur." status="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_MTjyMIVIEeCLO4soIxPKpg" name="Supprimer un modèle de planning" id="CU02_05">
        <requirements xmi:id="_AYZK8FPAEeGQtK6mgdcOyg" name="Droits : accès à supprimer les modèles de planning" id="CS_RGF_02_02_550" statement="Seuls les utilisateurs ayant un profil avec la fonction « créer, modifier, supprimer un modèle de planning  »  peuvent accéder à cette tâche">
          
          
          
        </requirements>
        <requirements xmi:id="__cxMUYVPEeCXi7kGgvvb2w" name="modèle de planning - Msg confirmation suppression modèle de planning" id="CS_RGF_02_103" statement="L'utilisateur doit confirmer sa volonté de supprimer un modèle de planning et est avisé par le message suivant :&#xD;&#xA;&quot;Confirmez-vous la suppression du modèle de planning suivant : &lt;libellé, code du modèle> ?&quot;." rationale="">
          
        </requirements>
        <requirements xmi:id="_NUpuIKlrEeCu6-1RnGqdag" name="Phase - Suppression modèle planning passage obsolète" id="CS_RGF_02_211" statement="Le système doit passer le modèle de planning dans l'état obsolète (mettre à &quot;TRUE&quot;).&#xD;&#xA;&#xD;&#xA;PS : attribut obsolète." rationale="">
          
        </requirements>
        <requirements xmi:id="_kVNj4LzsEeCd96ABgxeEzQ" name="Phase – initialisation de la date de fin de validité" id="CS_RGF_02_235" statement="La suppression du modèle initialise la date de fin de validité à la date du jour." rationale="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_X3Gt4IVIEeCLO4soIxPKpg" name="Gérer modèles de phases" id="CU02_06">
        <requirements xmi:id="_lBi5IFPAEeGQtK6mgdcOyg" name="Droits : accès à gérer modèles de phase" id="CS_RGF_02_02_650" statement="Seuls les utilisateurs ayant un profil avec la fonction « consulter les phases et les modèles de planning »  peuvent accéder à cette tâche">
          
          
          
        </requirements>
        <requirements xmi:id="_B6D6UIdiEeCeiKVfaqHkig" name="Phase - Règle de présentation du catalogue de phase" id="CS_RGF_02_113" statement="Le système doit récupérer et présenter le catalogue de phases trié par ordre croissant du type de phase, de la nature de phase puis du libellé abrégé de phase.&#xD;&#xA;Les colonnes de la liste sont :&#xD;&#xA;- Type de phase,&#xD;&#xA;- Nature de phase,&#xD;&#xA;- Libellé abrégé,&#xD;&#xA;- Obsolète." rationale="">
          
          
          
        </requirements>
        <requirements xmi:id="_4xX1IKbqEeCJBbE_RHaMLw" name="Phase - Règle de présentation du libellé de phase" id="CS_RGF_02_200" statement="Le système doit présenter un tooltip (au passage de la souris sur chaque ligne de la liste de phase) qui présente le libellé de la phase :&#xD;&#xA;&quot;Libellé :&quot; + Libellé de phase." rationale="">
          
          
          
        </requirements>
        <requirements xmi:id="_0kyMAKcCEeCJBbE_RHaMLw" name="Phase - Règle de présentation des phases obsolètes" id="CS_RGF_02_202" statement="Le système doit présenter les phases obsolètes en rouge sur demande dans la liste des phases et ne pas les rendre ni modifiable ni supprimable.&#xD;&#xA;&#xD;&#xA;" rationale="">
          
          
          
          
          
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_X3Gt4oVIEeCLO4soIxPKpg" name="Modifier une phase" id="CU02_07">
        <requirements xmi:id="_S6AE8FPAEeGQtK6mgdcOyg" name="Droits : accès à modifier une phase" id="CS_RGF_02_02_750" statement="Seuls les utilisateurs ayant un profil avec la fonction « créer, modifier, supprimer une phase  »  peuvent accéder à cette tâche">
          
          
        </requirements>
        <requirements xmi:id="_qd0UkFPAEeGQtK6mgdcOyg" name="Droits : accès à consulter une phase" id="CS_RGF_02_02_751" statement="Les utilisateurs ayant un profil avec la fonction « consulter les phases et les modèles de planning »  peuvent également accéder à cette tâche mais en lecture seule.">
          
          
        </requirements>
        <requirements xmi:id="_Lb4a8IVWEeCXi7kGgvvb2w" name="Phase - création phase" id="CS_RGF_02_105" statement="Le système doit vérifier qu'aucune autre phase (non obsolète) ne porte le même libellé ou le même libellé abrégé (indépendamment de la casse) ou sortir en erreur.">
          
          
          
        </requirements>
        <requirements xmi:id="_dH6X0IVWEeCXi7kGgvvb2w" name="Phase - Msg erreur création phase" id="CS_RGF_02_106" statement="Si le système ne peut créer de phase pour raison du libellé ou le même libellé abrégé existant, il doit proposer le message suivant dans une boîte de dialogue &quot;OK&quot; :&#xD;&#xA;&quot;Création impossible, une autre phase porte déjà ce libellé (ou ce libellé abrégé) , veuillez le modifier&quot;">
          
          
          
          
        </requirements>
        <requirements xmi:id="_Aly60IVZEeCXi7kGgvvb2w" name="Phase - confirmer modification une phase existante" id="CS_RGF_02_110" statement="La modification d'une phase utilisée par au moins un planning prévisionnel ou un modèle de planning doit être précédée d'une demande de confirmation de modification (avec une possibilité de notifier les utilisateurs concernés par les plannings prévisionnels).&#xD;&#xA;(Pour rappel la modification d'une phase qui a déjà été utilisée dans un planning prévisionnel n'impacte que la phase du catalogue et non celles du planning prévisionnel)&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_yzpC8KPrEeCpoffrgkIyag" name="Phase - type de phase condition" id="CS_RGF_02_111" statement="Le type et la nature de phase ne sont  modifiables qu'en création de la phase, ensuite ces champs sont inaccessibles mais visibles">
          
          
          
          
          
        </requirements>
        <requirements xmi:id="_oZG-0IXQEeCFSK46ESxCMA" name="Phase - Msg confirme modifier une phase existante" id="CS_RGF_02_112" statement="Le système présente le message de confirmation suivant :&#xD;&#xA;&quot;La phase en cours de modification est actuellement utilisée par les modèles de planning suivants :&lt;liste des modèles séparés par des virgules>. Confirmez-vous la modification (répondre « continuer ») ? Confirmez-vous la modification et voulez vous notifier les utilisateurs des plannings prévisionnels concernés (répondre « continuer et notifier ») ? (Pas d'impact sur les modèles de planning et plannings prévisionnels l'ayant recopiée)&quot;&#xD;&#xA;" rationale="" acceptanceCriteria="">
          
          
        </requirements>
        <requirements xmi:id="_c8SZ0IVaEeCXi7kGgvvb2w" name="Phase - création jalon de phase" id="CS_RGF_02_115" statement="Le système doit créer un jalon de début et un jalon de fin sur la phase créée avec un label par défaut respectivement &quot;début &lt;libellé phase>&quot;, &quot;fin &lt;libellé phase>&quot; (si le ou les libellés de jalons sont vides)  et positionner l'attribut &quot;franchi&quot; à &quot;non&quot;." rationale="">
          
          
          
          
        </requirements>
        <requirements xmi:id="_XmdkoIVaEeCXi7kGgvvb2w" name="Phase - consultation" id="CS_RGF_02_116" statement="Si une consultation est demandée, l'écran de modification de phase est en lecture seule.">
          
          
        </requirements>
        <requirements xmi:id="_Qr5pgJgiEeCFrZJTAzOlZg" name="Phase - initialisation de l'écran de modification" id="CS_RGF_02_117" statement="Le système doit récupérer l'ensemble des informations de la phase, y compris celles liées à ses deux jalons puis mettre à jour l'écran de modification de phase avec les valeurs récupérées.">
          
          
          
        </requirements>
        <requirements xmi:id="_dkrrEJgoEeCFrZJTAzOlZg" name="Phase - type de phase" id="CS_RGF_02_118" statement="Les types de phases possibles sont uniquement : &#xD;&#xA;- &quot;prévisionnelle&quot;&#xD;&#xA;- et &quot;jalon&quot;&#xD;&#xA;&#xD;&#xA;Pour information : une phase prévisionnelle est pour les utilisateurs hors SID alors que les jalons ne le sont que pour le SID.&#xD;&#xA;">
          
          
          
          
          
        </requirements>
        <requirements xmi:id="_BcWcgJgqEeCFrZJTAzOlZg" name="Phase - enregistrement phase créée" id="CS_RGF_02_119" statement="Le système doit créer :&#xD;&#xA;- une PhaseJalon si le type de phase qui a été choisi est de type &quot;Jalon&quot;,&#xD;&#xA;- une PhasePrevisionnelle si le type de phase qui a été choisi est de type &quot;Prévisionnelle&quot;.&#xD;&#xA;" rationale="">
          
          
          
          
        </requirements>
        <requirements xmi:id="_bryIYJgrEeCFrZJTAzOlZg" name="Phase - condition minimale de validation" id="CS_RGF_02_127" statement="La validation  de la création / modification d'une phase n'est possible et disponible à l'utilisateur que si le libellé abrégé, le libellé de la phase et la nature de phase et le type de phase sont renseignés et non vides.">
          
          
          
        </requirements>
        <requirements xmi:id="_lwfAIJguEeCFrZJTAzOlZg" name="Phase - modification phase" id="CS_RGF_02_128" statement="Le système doit vérifier qu'aucune autre phase ne porte le même libellé ou le même libellé abrégé ou sortir en erreur." rationale=""/>
        <requirements xmi:id="_d4S74JguEeCFrZJTAzOlZg" name="Phase - Msg erreur modification phase" id="CS_RGF_02_129" statement="Si le système ne peut modifier de phase pour raison du libellé ou du libellé abrégé existant, il doit proposer le message suivant dans une boîte de dialogue &quot;OK&quot; :&#xD;&#xA;&quot;Modification impossible, une autre phase porte déjà ce libellé (ou ce libellé abrégé), veuillez le modifier&quot;"/>
        <requirements xmi:id="_GniWcJjDEeC2N5fmUmQWNQ" name="Phase - nature de phase par défaut" id="CS_RGF_02_160" statement="La nature de phase est présentée vide par défaut à l'utilisateur.">
          
          
          
          
        </requirements>
        <requirements xmi:id="_fgLD8JjDEeC2N5fmUmQWNQ" name="Phase - init libellé vide avec nature de phase" id="CS_RGF_02_161" statement="Le champ est obligatoire, mettre par défaut dedans le libellé de la nature sélectionnée (si une nature est sélectionnée). Il est possible de changer ensuite ce libellé. &#xD;&#xA;Si la nature vient à changer et que le libellé de la phase est déjà renseigné, ne pas le modifier.&#xD;&#xA;">
          
          
          
          
          
        </requirements>
        <requirements xmi:id="_nVBkIKcYEeCJBbE_RHaMLw" name="Phase - notification modification phase" id="CS_RGF_02_203" statement="Si la notification a été demandée par l'utilisateur qui modifie la phase alors le système doit rendre la phase source obsolète et créer une nouvelle phase avec les nouveaux paramètres. " rationale="" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_p_6q0KcdEeCJBbE_RHaMLw" name="Phase - nature filtrée par type de phase" id="CS_RGF_02_205" statement="Les valeurs présentées à l'utilisateur du champs &quot;nature de phase&quot; dépendent du &quot;type de phase&quot; avec la règle suivante :&#xD;&#xA;Filtrer les natures des phases de type &quot;prévisionnelles&quot; avec l'exigence :&#xD;&#xA;CS_RGF_CP_021&#xD;&#xA;Filtrer les natures des phases de type &quot;jalons&quot; avec l'exigence :&#xD;&#xA;CS_RGF_CP_022&#xD;&#xA;&#xD;&#xA;NB : Il est à noter que si un basculement est effectué d'un type à l'autre lors de la création alors c'est la règle par défaut qui s'applique." rationale="">
          
          
        </requirements>
        <requirements xmi:id="_Ocs0IKciEeCJBbE_RHaMLw" name="Phase - enregistrement phase" id="CS_RGF_02_206" statement="Le système doit mettre à jour la phase ou la créer et renseigner la date de dernière modification avec la date du jour" rationale="">
          
          
          
          
          
        </requirements>
        <requirements xmi:id="_w8Cy8LRkEeCP0oKp7S9S2A" name="Phase - recherche de planning ayant été initialisé à phase modifiée" id="CS_RGF_02_221" statement="Si la modification de la phase est accompagnée d’une demande de notification (et donc qu’elle est utilisée soit dans un modèle de planning soit directement dans un planning prévisionnel) elle passe automatiquement à l’état obsolète et une nouvelle phase est créée faisant &#xD;&#xA;référence à la phase devenue obsolète.&#xD;&#xA;" rationale="" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_JHpb0LzrEeCd96ABgxeEzQ" name="Phase - initialisation date de création si obsolète" id="CS_RGF_02_230" statement="Si la phase devient obsolète et qu’une nouvelle phase est créée, cette dernière à pour date de création la date du jour.&#xD;&#xA;" rationale="" acceptanceCriteria="">
          
          
        </requirements>
        <requirements xmi:id="_O4D7QLzrEeCd96ABgxeEzQ" name="Phase - initialisation date de fin de validité" id="CS_RGF_02_231" statement="Si la phase devient obsolète sa date de fin de validité est initialisée à la date du jour." rationale="" acceptanceCriteria="">
          
          
        </requirements>
        <requirements xmi:id="_xCaEsLzrEeCd96ABgxeEzQ" name="Phase - initialisation date de création" id="CS_RGF_02_232" statement="A la création de la phase, le champ date de création est initialisé à la date du jour" rationale="" acceptanceCriteria="">
          
          
        </requirements>
        <requirements xmi:id="_AgHLgMzUEeCPfcn-az1AkQ" name="Phase – Libellé de jalons" id="CS_RGF_02_237" statement="Le système renseigne les libellé des jalons par « début » + libellé abrégé de la phase pour le jalon de début et « fin » + libellé abrégé de la phase pour le jalon de fin.&#xD;&#xA;Les libellés sont modifiables par l’utilisateur mais ne peuvent pas être vides.&#xD;&#xA;" rationale="">
          
          
          
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_X3Gt44VIEeCLO4soIxPKpg" name="Supprimer une phase" id="CU02_08">
        <requirements xmi:id="_zKHWUFPAEeGQtK6mgdcOyg" name="Droits : accès à supprimer une phase" id="CS_RGF_02_02_850" statement="Seuls les utilisateurs ayant un profil avec la fonction « créer, modifier, supprimer une phase  »  peuvent accéder à cette tâche">
          
        </requirements>
        <requirements xmi:id="_s-iWcIVPEeCXi7kGgvvb2w" name="Phase - Msg erreur suppression phase " id="CS_RGF_02_101" statement="Chaque suppression doit faire l’objet d’une confirmation par l'utilisateur :&#xD;&#xA;&quot;Voulez-vous supprimer cette phase ?&quot;.&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_W5Az8Id3EeCeiKVfaqHkig" name="Phase - Suppression phase dans catalogue" id="CS_RGF_02_107" statement="Le système doit désassocier  la phase du catalogue de phase auquel elle est liée." rationale="">
          
        </requirements>
        <requirements xmi:id="_oAdIwKcCEeCJBbE_RHaMLw" name="Phase - Suppression phase passage obsolète" id="CS_RGF_02_201" statement="Le système doit passer la phase du catalogue de phase dans l'état obsolète.&#xD;&#xA;&#xD;&#xA;PS : attribut obsolète." rationale="">
          
        </requirements>
        <requirements xmi:id="_J_t20LzsEeCd96ABgxeEzQ" name="Phase – initialisation date de fin de validité" id="CS_RGF_02_233" statement="La date de fin de validité de la phase est initialisée à la date du jour" rationale="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_JxFAwJJhEeC4A-726iExUQ" name="Ajouter / retirer phase dans modèle de planning" id="CU02_09">
        <requirements xmi:id="_MCKDYFPAEeGQtK6mgdcOyg" name="Droits : accès à ajouter / retirer phase dans modèle de planning" id="CS_RGF_02_02_950" statement="Seuls les utilisateurs ayant un profil avec la fonction « créer, modifier, supprimer un modèle de planning  »  peuvent accéder à cette tâche">
          
        </requirements>
        <requirements xmi:id="_GG8hwIdzEeCeiKVfaqHkig" name="Modèle Planning - présentation de la liste des phases" id="CS_RGF_02_125" statement="Le système doit présenter la liste des phases dans un tableau ordonné par ordre croissant sur le rang de la phase dans le modèle avec les informations complémentaires suivantes : libellé abrégé de la phase, libéllé de la phase, type de phase, ou type de phase est défini comme suit :&#xD;&#xA;- &quot;Jalon&quot; pour une PhaseJalon,&#xD;&#xA;-  &quot;Prévisionnelle&quot; pour une PhasePrevisionnelle.&#xD;&#xA;">
          
          
        </requirements>
        <requirements xmi:id="_qak2cId0EeCeiKVfaqHkig" name="Modèle Planning - tri par défaut" id="CS_RGF_02_163" statement="Le système doit présenter par défaut la liste de phases triée par libellé abrégé croissant." rationale="">
          
          
        </requirements>
        <requirements xmi:id="_KapiMJJhEeC4A-726iExUQ" name="Modèle Planning - unicité de phase" id="CS_RGF_02_164" statement="Le système ne doit pas lier deux fois la même phase et donc filtrer les phases proposées pour ne pas présenter une phase déjà liée.">
          
        </requirements>
        <requirements xmi:id="_vL9qkJ2pEeCozcSUZ3dsfg" name="Ajouter / retirer phase dans modèle de planning - cond de rehaussement" id="CS_RGF_02_170" statement="Le système ne doit rendre disponible le rehaussement d'une phase dans la liste des phases que si la phase à rehausser n'est pas en plus haute position et que ce n'est pas la seule phase de la liste.">
          
        </requirements>
        <requirements xmi:id="_DVztsJ2qEeCozcSUZ3dsfg" name="Ajouter / retirer phase dans modèle de planning - cond d'abaissement" id="CS_RGF_02_171" statement="Le système ne doit rendre disponible ll'abaissement d'une phase dans la liste des phases que si la phase à abaisser n'est pas en plus basse position et que ce n'est pas la seule phase de la liste.">
          
        </requirements>
        <requirements xmi:id="_JLV9sJ2qEeCozcSUZ3dsfg" name="Ajouter / retirer phase dans modèle de planning - ré ordonnancement" id="CS_RGF_02_172" statement="Le système ne doit réordonnancer les phases de la liste sur un ajout qui peut être après ou avant et sur un rehaussement ou un abaissement d'une phase dans la liste puis actualiser la liste des phases." rationale="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_0R3ekKu5EeC3BJHyO2WSvw" name="Ajouter phases" id="CU02_10">
        <requirements xmi:id="_47v9wFPAEeGQtK6mgdcOyg" name="Droits : accès à ajouter phases" id="CS_RGF_02_02_051" statement="Seuls les utilisateurs ayant un profil avec la fonction « créer, modifier, supprimer une phase  »  peuvent accéder à cette tâche">
          
        </requirements>
        <requirements xmi:id="_0UxhQKu8EeC3BJHyO2WSvw" name="Ajouter phases - Règle de présentation des phases pour sélection" id="CS_RGF_02_214" statement="Le système doit présenter au moyen du composant LOV les modèles de phase :&#xD;&#xA;•&#x9;Sélection multiple&#xD;&#xA;•&#x9;Résultat : triés par ordre croissant du type de phase, la nature de phase puis du libellé abrégé de phase.&#xD;&#xA;•&#x9;Champs de recherche : Type de phase, Nature de phase et libellé abrégé&#xD;&#xA;Champs affichés : &#xD;&#xA;- Type de phase,&#xD;&#xA;- Nature de phase,&#xD;&#xA;- Libellé abrégé.&#xD;&#xA;Le nombre de ligne par page par défaut est de 50.&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_zKTHoKu7EeC3BJHyO2WSvw" name="Ajouter phases - Récupérer phases pour ajout" id="CS_RGF_02_215" statement="Le système doit rechercher la liste des modèles de phases possibles (et les informations de Type de phase,  Nature de phase, Libellé abrégé et Libellé de phase).&#xD;&#xA;Ces phases sont celles qui : &#xD;&#xA;- ne sont pas obsolètes,&#xD;&#xA;- n'appartiennent pas déjà au modèle de planning.&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_0YgxwKu8EeC3BJHyO2WSvw" name="Ajouter phases - gérer modèles de phase" id="CS_RGF_02_216" statement="Le système doit permettre d'appeler la gestion des modèles de phase de manière modale (ce qui bloque l'accès à l'écran &quot;ajouter retirer phases&quot;  et aux modèle de planning en cours de création modification).&#xD;&#xA;En sortie de la gestion des modèles de phase, on doit récupérer la main sur  l'écran &quot;ajouter retirer phases&quot;.&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_0hd5cKu8EeC3BJHyO2WSvw" name="Ajouter phases - Règle de mise à jour de l'écran des phases" id="CS_RGF_02_217" statement="Sur retour de la tâche &quot;gérer modèles de phase&quot;, la liste des phases doit être mise à jour.">
          
        </requirements>
        <requirements xmi:id="_0mF6wKu8EeC3BJHyO2WSvw" name="Ajouter phases - Règle de validation de phases à ajouter" id="CS_RGF_02_218" statement="Le système nécessite qu'au moins une phase soit sélectionnée et permet une sélection multiple de phases.">
          
        </requirements>
        <requirements xmi:id="_EMMQcKu-EeC3BJHyO2WSvw" name="Ajouter phases - Règle de mise à jour des phases sélectionnées dans le modèle de planning" id="CS_RGF_02_219" statement="Le système doit mettre à jour la liste des phases du modèle de planning avec la sélection multiple de phases qu'il conservera dans l'ordre de la liste des modèles des phases et qui positionnera soit avant la phase sélectionnée de la liste des phases du modèle de planning (cette information arrive en entrée de cette tâche) ou en fin de liste si aucune phase n’est sélectionnée">
          
        </requirements>
        <requirements xmi:id="_O33eEKvAEeC3BJHyO2WSvw" name="Ajouter phases - Règle de présentation libéllé de phase" id="CS_RGF_02_220" statement="Le système doit présenter un tooltip (au passage de la souris sur chaque ligne de la liste des phases) qui présente le libellé du modèle de phase de la liste :&#xD;&#xA;&quot;Libellé :&quot; + Libellé du modèle de phase.&#xD;&#xA;">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_KPDkIDYeEeG__oHhivFwHw" name="Navigateur" id="CU02_11">
        <requirements xmi:id="_7mIWcDYeEeG__oHhivFwHw" name="Navigateur : Affichage de l'arborescence de travail des phases de référence" id="CS_RGF_02_02_501" statement="L'arborescence affichée est consitutée du catalogue des phases de référence du SID en affichant le libellé abrégé de la phase. (le libellé long apparaissant en info-bulle). Elle comporte un noeud nommé &quot;Catalogue des phases&quot;">
          
          
          
        </requirements>
        <requirements xmi:id="_RgWVkDYgEeG__oHhivFwHw" name="Navigateur : Choix d'affichage phases de référence : Type de phase" id="CS_RGF_02_02_502" statement="L'utilisateur peut choisir que les phases du catalogue soient regroupées en fonction de leur type.&#xD;&#xA;(Jalon, Prévisionnelle) et/ou de leur nature (Approbation conception, Remise à l'utilisateur...) L'arborescence de travail sera présentée avec des noeuds correspondant aux valeurs des types et des natures des phases qui seront regroupées dans ces noeuds.">
          
          
          
        </requirements>
        <requirements xmi:id="_RgWVkjYgEeG__oHhivFwHw" name="Navigateur : Choix d'affichage phases de référence : Obsolète" id="CS_RGF_02_02_504" statement="L'utilisateur peut choisir que les phases qui sont obsolètes puissent apparaître dans le catalogue avec une icone spécifique.&#xD;&#xA;Le libellé abrégé de la phase obsolète doit être en rouge.&#xD;&#xA;&#xD;&#xA;Dans l'arborescence de travail la phase obsolète sera positionnée :&#xD;&#xA;&#xD;&#xA;soit en dessous de la phase qui la remplace si on a procédé à une modification de la phase&#xD;&#xA;soit conformément à la règle CS_RGF_02_113" status="">
          
          
          
        </requirements>
        <requirements xmi:id="_DLHXIDapEeGCNrPCrWWKng" name="Navigateur : Affichage de l'arborescence de travail des Modèles de planning" id="CS_RGF_02_02_505" statement="Si l'administrateur des modèles de planning possède des droits de niveau 1 (SID), l'arborescence affichée est constituée du catalogue des modèles de planning du SID en affichant le libellé abrégé du modèle de planning (le libellé long apparaissant en info-bulle), ainsi que les catalogues des organismes de niveau 2 (ESID/DID).&#xD;&#xA;&#xD;&#xA;Si l'administrateur possède des droits de niveau 2 (ESID/DID), l'arborescence affichée est constituée du catalogue des modèles de planning de l'ESID de rattachement de l'organisme de l'utilisateur en affichant le libellé abrégé du modèle de planning (le libellé long apparaissant en info-bulle)&#xD;&#xA;&#xD;&#xA;L'arborescence des modèles de planning comporte un noeud nommé &quot;Catalogue des modèles de planning&quot;">
          
          
          
        </requirements>
        <requirements xmi:id="_DLHXITapEeGCNrPCrWWKng" name="Navigateur : Choix d'affichage modèles de planning : Type de MOE" id="CS_RGF_02_02_506" statement="L'utilisateur peut choisir que les modèles de planning de son(ses) catalogue(s) soient regroupés en fonction du type de MOE. (Interne, Privée, Mixte) et/ou de la nature des opérations auxquelles les modèles sont rattachés (Acquisition, construction, démolition...) et/ou du code des modèles de planning&#xD;&#xA;&#xD;&#xA;L'arborescence de travail sera présentée avec des noeuds correspondant aux valeurs des types MOE, des natures d'opérations et des codes des modèles de plannings qui seront regroupées dans ces noeuds.">
          
          
          
        </requirements>
        <requirements xmi:id="_DLHXIzapEeGCNrPCrWWKng" name="Navigateur : Choix d'affichage modèles de planning : Obsolète" id="CS_RGF_02_02_508" statement="L'utilisateur peut choisir que les modèles de planning qui sont obsolètes puissent apparaître dans le(s) catalogue(s) avec une icone spécifique.&#xD;&#xA;Le libellé abrégé du modèle de planning obsolète doit être en rouge.&#xD;&#xA;&#xD;&#xA;Dans l'arborescence de travail le modèle de planning obsolète sera positionné :&#xD;&#xA;&#xD;&#xA;soit en dessous du modèle de planning qui le remplace si on a procédé à une modification du modèle de planning&#xD;&#xA;soit conformément à la règle CS_RGF_02_120" status="">
          
          
          
        </requirements>
        <requirements xmi:id="_PW7h0DarEeGCNrPCrWWKng" name="Navigateur : Administrer COSI : choix du type d'éléments à administrer" id="CS_RGF_02_02_510" statement="La liste &quot;choix des éléments&quot; située au dessus de l'arborescence de travail, permet à l'utilisateur de choisir le type d'éléments métier qu'il souhaite administrer.&#xD;&#xA;&#xD;&#xA;Les types d'éléments de ce panel sont visibles et disponibles à l'utilisateur en fonction de ses droits." rationale="" status="">
          
          
        </requirements>
        <requirements xmi:id="_QWbagDasEeGCNrPCrWWKng" name="Navigateur : Affichage des barres d'outils" id="CS_RGF_02_02_511" statement="L'écran affiche dynamiquement trois barres d'outils en fonction du choix de l'élément métier à administrer:&#xD;&#xA;- Phase de référence: comportant les boutons (ajouter, éditer, supprimer et consulter)&#xD;&#xA;- Modèles de planning : comportant les boutons (ajouter, éditer, supprimer, consulter et gérer les phases)&#xD;&#xA;- Listes de référence : comportant les boutons (ajouter, éditer, supprimer et consulter)&#xD;&#xA;" status="">
          
          
          
          
        </requirements>
        <requirements xmi:id="_PfmSQT3VEeGSvZ0BDBzUEg" name="Navigateur : Contenu zone de détail sur création modification de phases" id="CS_RGF_02_02_512" statement="La zone de détail contient :&#xD;&#xA;&#xD;&#xA;Si création de phase : Rien&#xD;&#xA;&#xD;&#xA;Si modification de phase : le libellé long de la phase, le type de phase, la nature de la phase et la date de dernière modification&#xD;&#xA;">
          
          
        </requirements>
        <requirements xmi:id="_TBMUQD3WEeGSvZ0BDBzUEg" name="Navigateur : Contenu zone de détail sur création modification de modèle de planning" id="CS_RGF_02_02_513" statement="La zone de détail contient :&#xD;&#xA;&#xD;&#xA;Si création de modèle de planning : Rien&#xD;&#xA;&#xD;&#xA;Si modification de modèle de planning :le libellé, libellé abrégé et code du modèle de planning et le type MOE, la Nature d'opération et le commentaire" status="">
          
          
        </requirements>
        <requirements xmi:id="_OczKME54EeG3heEAk9xgAQ" name="Navigateur : Contenu zone de détail sur création modification de listes de référence" id="CS_RGF_02_02_514" statement="La zone de détail contient :&#xD;&#xA;- Si création de liste de référence : Rien&#xD;&#xA;- Si modification de listes de référence : le libellé, l'identifiant de liste (c'est-à-dire le libellé abrégé de l'organisme + le rang)  et le commentaire" status="">
          
          
          
          
          
          
        </requirements>
        <requirements xmi:id="_LFDV0E55EeG3heEAk9xgAQ" name="Navigateur : Affichage de l'arborescence de travail des listes de référence" id="CS_RGF_02_02_515" statement="L’arborescence affichée est constituée du catalogue des listes de références du SID en affichant le libellé abrégé de la liste de référence (le libellé long apparaissant en info-bulle), ainsi que les catalogues des organismes de niveau 2 (ESID/DID) et de niveau 3 (USID) selon le périmètre géographique accessible à l’utilisateur. Elle comporte un nœud principal nommé &quot;Catalogue des listes de référence&quot;.">
          
          
          
          
        </requirements>
        <requirements xmi:id="_MJLP0E56EeG3heEAk9xgAQ" name="Navigateur : Affichage de l'arborescence de travail des listes de référence : Projets et opérations" id="CS_RGF_02_02_516" statement="L'utilisateur peut choisir que les projets et les opérations puissent apparaître dans le(s) catalogue(s) en tant que noeuds enfants des listes de référence auxquels ils sont appartiennent.&#xD;&#xA;L'arborescence qui en découle respecte les règles d'affichage des listes de travail du CU46 relatives à l'affichage des projets et des opérations.">
          
          
          
          
          
        </requirements>
        <requirements xmi:id="_vA7jwE54EeG3heEAk9xgAQ" name="Navigateur : Tri avancé listes de référence : Organismes" id="CS_RGF_02_02_517" statement="L'utilisateur peut choisir que les listes de référence du catalogue soient triées en fonction de leur organisme de niveau 2 ou de leur organisme de niveau 3.&#xD;&#xA;L'arborescence de travail sera présentée avec des noeuds correspondant aux organismes, ces nœuds regroupant les listes de références d’un même organisme.">
          
          
          
          
        </requirements>
        <requirements xmi:id="_lryHYE56EeG3heEAk9xgAQ" name="Navigateur : Choix d'affichage listes de référence : Marchés" id="CS_RGF_02_02_518" statement="L'utilisateur peut choisir que les listes de référence du catalogue soient regroupées en fonction des marchés dont elles dépendent.&#xD;&#xA;L'arborescence de travail sera présentée avec des noeuds correspondant aux marchés, ces nœuds regroupant les listes de références d’un même marché.">
          
          
          
          
        </requirements>
        <requirements xmi:id="_x4OrkE55EeG3heEAk9xgAQ" name="Navigateur : Choix d'affichage listes de référence : Code" id="CS_RGF_02_02_519" statement="L'utilisateur peut choisir que le libellé des éléments de chaque noeud soit précédé de leur code.">
          
          
          
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_65yNkDa9EeGCNrPCrWWKng" name="Associer les fonctions" id="CU02_12">
        <requirements xmi:id="_rgF_YDbAEeGCNrPCrWWKng" name="Fonction : consulter les phases et les modèles de plannings" id="CS_RGF_02_02_600" statement="La fonction &quot;consulter les phases et les modèles de plannings&quot; permet de rendre visibles en lecture les catalogues de phases et de modèles de plannings en fonction du niveau défini pour l'utilisateur" rationale="" status="">
          
          
        </requirements>
        <requirements xmi:id="_YeeYgDbUEeGCNrPCrWWKng" name="Fonction : créer modifier supprimer modèle de planning" id="CS_RGF_02_02_601" statement="La fonction créer, modifier supprimer modèle de planning permet de rendre disponible en modification, création et suppression les modèles de planning ainsi que d'ajouter ou retirer des phases dans ces modèles.&#xD;&#xA;&#xD;&#xA;L’ajout de cette fonction au profil de l’utilisateur n’est possible que s’il possède déjà la fonction &quot;consulter les phases et les modèles de planning&quot;" rationale="" status="">
          
        </requirements>
        <requirements xmi:id="_k3ZAgDbVEeGCNrPCrWWKng" name="Fonction : créer modifier supprimer les phases de référence" id="CS_RGF_02_02_602" statement="La fonction créer, modifier supprimer les phases de référence,  permet de rendre disponible en modification, création et suppression les phases de référence&#xD;&#xA;&#xD;&#xA;L’ajout de cette fonction au profil de l’utilisateur n’est possible que s’il possède déjà la fonction &quot;consulter les phases et les modèles de planning&quot;" rationale="" status="">
          
          
        </requirements>
        <requirements xmi:id="_WJ2K4E53EeG3heEAk9xgAQ" name="Fonction : consulter les listes de référence" id="CS_RGF_02_02_603" statement="La fonction &quot;consulter les listes de référence&quot; permet de rendre visibles en lecture les catalogues de listes de référence en fonction du niveau défini pour l'utilisateur."/>
        <requirements xmi:id="_l2mXoE53EeG3heEAk9xgAQ" name="Fonction : créer, modifier, supprimer, affecter  les listes de référence" id="CS_RGF_02_02_604" statement="La fonction &quot;créer, modifier, supprimer, affecter les listes de référence&quot; permet de créer, modifier, supprimer ou les listes de référence à des organismes en fonction du niveau défini pour l'utilisateur.&#xD;&#xA;L’ajout de cette fonction au profil de l’utilisateur n’est possible que s’il possède déjà la fonction &quot;consulter les listes de référence&quot;"/>
      </subCategories>
      <subCategories xmi:id="_B32RUE53EeG3heEAk9xgAQ" name="Gérer les listes de références" id="CU02_13">
        <requirements xmi:id="_EXPvIE57EeG3heEAk9xgAQ" name="Droits : accès à gérer les listes de référence" id="CS_RGF_02_02_300" statement="Seuls les utilisateurs ayant un profil avec la fonction « consulter les listes de référence »  peuvent accéder à cette tâche">
          
        </requirements>
        <requirements xmi:id="_eR_Z4E57EeG3heEAk9xgAQ" name="Droits : accès à modifier les listes de référence" id="CS_RGF_02_02_301" statement="Seuls les utilisateurs ayant un profil avec la fonction « créer, modifier, supprimer, affecter les listes de référence »  peuvent accéder à cette tâche.">
          
          
        </requirements>
        <requirements xmi:id="_8CYrsFCYEeG13P2rR7U2MA" name="Droits : accès à consulter les listes de référence" id="CS_RGF_02_02_302" statement="Les utilisateurs ayant un profil avec la fonction « consulter les listes de référence »  peuvent également accéder à cette tâche mais en lecture seule.">
          
          
          
          
        </requirements>
        <requirements xmi:id="_IsMrQE59EeG3heEAk9xgAQ" name="Listes de référence" id="CS_RGF_02_02_303" statement="En plus de la liste par défaut, tous les utilisateurs d'un même organisme disposent des listes de référence qui ont été affectés à cet organisme. Ces listes ne sont ni supprimables ni modifiables par les utilisateurs. Leur nombre n'est pas limité. Elles peuvent être placées dans les 5 favoris.">
          
          
        </requirements>
        <requirements xmi:id="_P0KMcE59EeG3heEAk9xgAQ" name="Listes de référence : affichage" id="CS_RGF_02_02_304" statement="La liste de référence est affichée dans le catalogue de la manière suivante : libellé abrégé de l'organisme de l'administrateur + rang + : + libellé&#xD;&#xA;exemple : &quot;ESID_LYO12 : démolitions 2010&quot;">
          
          
          
          
          
        </requirements>
        <requirements xmi:id="_QIHqQE59EeG3heEAk9xgAQ" name="Listes de référence : rang" id="CS_RGF_02_02_305" statement="les listes de référence, créées par l'administrateur d'organisme possèdent un rang de 01 à 99. Le rang des listes de référence doit toujours se trouver dans la plage 01-99. Lorsqu'une liste de référence est supprimée, son rang est réutilisable pour la prochaine liste créée. Le rang 00 est réservé à la liste par défaut.">
          
          
          
          
        </requirements>
        <requirements xmi:id="_mQnHwFPVEeGQtK6mgdcOyg" name="Listes de référence : rang - réutilisation après suppression" id="CS_RGF_02_02_306" statement="Lorsqu'une liste de référence est supprimée, son rang est réutilisable pour la prochaine liste créée.">
          
          
          
        </requirements>
        <requirements xmi:id="_ywyq0FPVEeGQtK6mgdcOyg" name="Listes de référence : liste non supprimable" id="CS_RGF_02_02_307" statement="La liste par défaut n’est pas supprimable">
          
        </requirements>
        <requirements xmi:id="_NGYegFPWEeGQtK6mgdcOyg" name="Listes de référence : mode consultation" id="CS_RGF_02_02_308" statement="L'accès en consultation des listes de référence permet de visualiser l'ensemble des écrans accessibles en modification mais avec les champs grisés, non modifiable et les boutons d'enregistrement désactivés.">
          
          
          
          
        </requirements>
        <requirements xmi:id="_1dFnEFPXEeGQtK6mgdcOyg" name="Listes de référence : catalogue" id="CS_RGF_02_02_309" statement="Le catalogue des listes de référence est constitué de l’ensemble des listes  de référence dans la limite du périmètre géographique des droits de consultation des listes de référence de l’utilisateur.">
          
          
          
        </requirements>
        <requirements xmi:id="_TPOCAGd_EeG56tPgw1av2w" name="Listes de référence : catalogue - affichage" id="CS_RGF_02_02_310" statement="L'arborescence présente les listes regroupées par leurs organismes de diffusion, en respectant l'organisation hiérarchique du SID et par ordre croissant.">
          
        </requirements>
        <requirements xmi:id="_pU0nMGd_EeG56tPgw1av2w" name="Listes de référence : organisme de diffusion obligatoire" id="CS_RGF_02_02_311" statement="Une liste de référence doit contenir au moins un organisme de diffusion, en création ou en modification.">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_RuPlcE57EeG3heEAk9xgAQ" name="Affecter une liste de référence" id="CU02_14">
        <requirements xmi:id="_b0VrsE57EeG3heEAk9xgAQ" name="Droits : accès à affecter les listes de référence" id="CS_RGF_02_02_400" statement="Seuls les utilisateurs ayant un profil avec la fonction « créer, modifier, supprimer, affecter les listes de référence »  peuvent accéder à cette tâche">
          
          
          
          
        </requirements>
        <requirements xmi:id="_QaJ1cE59EeG3heEAk9xgAQ" name="Listes de référence : affectation de la liste aux organismes" id="CS_RGF_02_02_401" statement="l'utilisateur ayant les droits de créer des listes de référence doit pouvoir les affecter à des organismes de niveau 2 ou de niveau 3, pour les rendre disponibles à tous les utilisateurs de ces organismes.">
          
        </requirements>
        <requirements xmi:id="_Qvw5EE59EeG3heEAk9xgAQ" name="Listes de référence : choisir des organismes" id="CS_RGF_02_02_402" statement="Si l'utilisateur qui crée la liste de référence est du niveau 2 (ESID) il a le choix de diffuser sa liste vers les utilisateurs de l'ESID et vers les utilisateurs de certains ou tous les organismes de niveau 3.&#xD;&#xA;&#xD;&#xA;Si l'utilisateur qui crée la liste de référence est du niveau 3 (USID, PCO...), il ne peut diffuser sa liste qu'aux utilisateurs de son propre organisme.">
          
        </requirements>
        <requirements xmi:id="_6ZH4QHQNEeGRV_PQxm_bOg" name="Listes de référence : LOV arborescente organismes" id="CS_RGF_02_02_403" statement="LOV arborescente - organismes de diffusion&#xD;&#xA;- Sélection multiple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Restriction:&#xD;&#xA;Seuls les organismes appartenant au périmètre géographique des droits d’affectation de l’utilisateur peuvent être sélectionnés.&#xD;&#xA;Résultat :&#xD;&#xA;- trié par libellé abrégé croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- libellé abrégé (RF_ORGANISME_LIBA)&#xD;&#xA;- id organisme (RF_ORGANISME_ID)&#xD;&#xA;- Type organisme (RF_TORG_LIB)&#xD;&#xA;Champs retournés :&#xD;&#xA;- id organisme (RF_ORGANISME_ID)&#xD;&#xA;- libellé abrégé de l'organisme (RF_ORGANISME_LIBA)">
          
        </requirements>
        <requirements xmi:id="_R_WK0FPREeGQtK6mgdcOyg" name="Listes de référence : affectation par défaut" id="CS_RGF_02_02_404" statement="A l’ouverture de l’écran de création d’une liste de référence, l'organisme de l'administrateur est automatiquement pré-selectionnée dans la liste des organismes de diffusion. Il peut ensuite modifier cette affectation." status="">
          
        </requirements>
        <requirements xmi:id="_xhTgcGd_EeG56tPgw1av2w" name="Listes de référence : bouton supprimer" id="CS_RGF_02_02_405" statement="La fenêtre dispose d’un bouton « supprimer » qui permet de retirer un organisme de la liste des organismes de diffusion.lorsque cet organisme est sélectionné dans le tableau.">
          
        </requirements>
        <requirements xmi:id="_yVwcMGd_EeG56tPgw1av2w" name="Listes de référence : bouton supprimer - accessibilité" id="CS_RGF_02_02_406" statement="Lorsqu’il n’existe qu’un seul organisme de diffusion dans la liste, le bouton « supprimer » est inaccessible.">
          
        </requirements>
        <requirements xmi:id="_ytcjcGd_EeG56tPgw1av2w" name="Listes de référence : organismes dépendants" id="CS_RGF_02_02_407" statement="Les organismes sélectionnés pour la diffusion et qui dépendent hiérarchiquement d’un autre organisme lui aussi sélectionné pour la diffusion, sont automatiquement supprimés de la liste de diffusion.">
          
        </requirements>
        <requirements xmi:id="__PzIwHQMEeGRV_PQxm_bOg" name="Listes de référence : mode dégradé – remplacement du mode de sélection" id="CS_RGF_02_02_1451" statement="La LOV arborescente est remplacée en mode dégradé par une LOV de sélection de niveau 2 sans restriction de périmètre géographique et par une case à cocher de diffusion à tous">
          
        </requirements>
        <requirements xmi:id="__r85QHQMEeGRV_PQxm_bOg" name="Listes de référence : mode dégradé - LOV organismes" id="CS_RGF_02_02_1452" statement="LOV - organismes de diffusion&#xD;&#xA;- Sélection multiple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Restriction:&#xD;&#xA;Seuls les organismes de niveau 2 peuvent être sélectionnés.&#xD;&#xA;Résultat :&#xD;&#xA;- trié par libellé abrégé croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- id organisme (RF_ORGANISME_ID)&#xD;&#xA;- libellé abrégé (RF_ORGANISME_LIBA)&#xD;&#xA;- Type organisme (RF_TORG_LIB)&#xD;&#xA;Champs retournés :&#xD;&#xA;- id organisme (RF_ORGANISME_ID)&#xD;&#xA;- libellé abrégé de l'organisme (RF_ORGANISME_LIBA)">
          
        </requirements>
        <requirements xmi:id="_AMbwIHQNEeGRV_PQxm_bOg" name="Listes de référence : mode dégradé – case à cocher de diffusion à tous" id="CS_RGF_02_02_1453" statement="Une case à cocher « Visible pour tous » est ajoutée dans le mode dégradé, permettant de sélectionner la DCSID, organisme de niveau 1, et donc de diffuser à l’ensemble des organismes de niveau 2.">
          
        </requirements>
        <requirements xmi:id="_BCOvsHQNEeGRV_PQxm_bOg" name="Listes de référence : mode dégradé – diffuser à tous" id="CS_RGF_02_02_1454" statement="Lorsque la  case à cocher « Visible pour tous   est cochée, elle devient inactive, l’ensemble des organismes de niveau 2 déjà sélectionnés est supprimé de la liste et la DCSID est ajoutée à la liste. De plus, le bouton de sélection des organismes de diffusion devient également inactif.">
          
        </requirements>
        <requirements xmi:id="_Bdo5UHQNEeGRV_PQxm_bOg" name="Listes de référence : mode dégradé – suppression de la DCSID" id="CS_RGF_02_02_1455" statement="Lorsque la DCSID est supprimée de la liste de diffusion, la case à cocher redevient active mais décochée. De plus, le bouton de sélection des organismes de diffusion devient également actif.">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_cGjjIJs8EeCaWIZ-uoUj9A" name="Users Stories CU02" id="USCU02">
        <requirements xmi:id="_pmeeYJtcEeCaWIZ-uoUj9A" name="Gérer modèles phases / jalons" id="CU02_US05" statement="L'utilisateur veut pouvoir gérer les modèles de phase afin de pouvoir renseigner un catalogue de phases disponibles lors de la création / modification d'un modèle de planning.&#xD;&#xA;&#xD;&#xA;Scénario :&#xD;&#xA;Ce scénario débute dans la tâche &quot;Administrer COSI&quot;, par  l’User Action &quot;Gérer modèles phases / jalons&quot;,&#xD;&#xA;Ce scénario concerne la tâche &quot;Gérer modèles phases / jalons&quot;" rationale="Pré-requis :&#xD;&#xA;Le système a présenté à l’utilisateur le formulaire de &quot;Gestion modèles phases / jalons&quot;.&#xD;&#xA;&#xD;&#xA;" acceptanceCriteria=""/>
        <requirements xmi:id="_knQ_8Js8EeCaWIZ-uoUj9A" name="Créer une phase" id="CU02_US01" statement="L'utilisateur veut pouvoir créer une phase afin de pouvoir renseigner un catalogue de phases disponibles lors de la création / modification d'un modèle de planning.&#xD;&#xA;&#xD;&#xA;Scénario :&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer modèles phases / jalons&quot;, par  l’User Action &quot;Créer une phase&quot;,&#xD;&#xA;Ce scénario concerne la tâche &quot;Créer une phase&quot;" rationale="Pré-requis :&#xD;&#xA;Le système a présenté à l’utilisateur le formulaire de &quot;Gestion modèles phases / jalons&quot;.&#xD;&#xA;L'utilisateur demande à créer une phase&#xD;&#xA;&#xD;&#xA;" acceptanceCriteria=""/>
        <requirements xmi:id="_OQtYMJs9EeCaWIZ-uoUj9A" name="Modifier une phase" id="CU02_US02" statement="L'utilisateur veut pouvoir modifier une phase afin de pouvoir mettre à jour un catalogue de phases disponibles lors de la création / modification d'un modèle de planning.&#xD;&#xA;&#xD;&#xA;Scénario :&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer modèles de phases&quot;, par  l’User Action &quot;Modifier une phase&quot;,&#xD;&#xA;Ce scénario concerne la tâche &quot;Modifier une phase&quot;" rationale="Pré-requis :&#xD;&#xA;Le système a présenté à l’utilisateur le formulaire de &quot;Gestion modèles de phases&quot;.&#xD;&#xA;Au moins une phase existe en base de données.&#xD;&#xA;L'utilisateur demande à modifier une phase&#xD;&#xA;" acceptanceCriteria=""/>
        <requirements xmi:id="_OS8M0Js9EeCaWIZ-uoUj9A" name="Consulter une phase" id="CU02_US03" statement="L'utilisateur veut pouvoir consulter une phase afin de pouvoir g��rer les modèles de phases.&#xD;&#xA;&#xD;&#xA;Scénario :&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer modèles de phases&quot;, par  l’User Action &quot;Consulter une phase&quot;,&#xD;&#xA;Ce scénario concerne la tâche &quot;Consulter une phase" rationale="Pré-requis :&#xD;&#xA;Le système a présenté à l’utilisateur le formulaire de &quot;Gérer modèles de phases&quot;.&#xD;&#xA;Au moins une phase existe en base de données.&#xD;&#xA;L'utilisateur demande à consulter une phase&#xD;&#xA;&#xD;&#xA;&quot;" acceptanceCriteria=""/>
        <requirements xmi:id="_OUu8kJs9EeCaWIZ-uoUj9A" name="Supprimer une phase" id="CU02_US04" statement="L'utilisateur veut pouvoir supprimer une phase afin de pouvoir mettre à jour un catalogue de phases disponibles lors de la création / modification d'un modèle de planning.&#xD;&#xA;&#xD;&#xA;Scénario :&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer modèles de phases&quot;, par  l’User Action &quot;Supprimer une phase&quot;,&#xD;&#xA;Ce scénario concerne la tâche &quot;Supprimer une phase&quot;" rationale="Pré-requis :&#xD;&#xA;Le système a présenté à l’utilisateur le formulaire de &quot;Gérer modèles de phases&quot;.&#xD;&#xA;Au moins une phase existe en base de données.&#xD;&#xA;L'utilisateur demande à supprimer une phase&#xD;&#xA;&#xD;&#xA;" acceptanceCriteria=""/>
        <requirements xmi:id="_3yDMQJtcEeCaWIZ-uoUj9A" name="Gérer les modèles de planning" id="CU02_US15" statement="L'utilisateur veut pouvoir gérer les modèles de planning afin de pouvoir renseigner un catalogue modèles de planning disponibles afin de permettre l'initialisation de planning prévisionnels.&#xD;&#xA;&#xD;&#xA;Scénario :&#xD;&#xA;Ce scénario débute dans la tâche &quot;Administrer COSI&quot;, par  l’User Action &quot;Gérer des modèles de planning&quot;,&#xD;&#xA;Ce scénario concerne la tâche &quot;Gérer des modèles de planning&quot;" rationale="Pré-requis :&#xD;&#xA;Le système a présenté à l’utilisateur le formulaire de &quot;Gestion de modèles de planning&quot;.&#xD;&#xA;&#xD;&#xA;" acceptanceCriteria=""/>
        <requirements xmi:id="_zZ__AJs9EeCaWIZ-uoUj9A" name="Créer un modèle de planning" id="CU02_US11" statement="L'utilisateur veut pouvoir créer un modèle de planning afin de pouvoir renseigner un catalogue de modèles de planning disponibles lors de l'initialisation d'un planning prévisionnel.&#xD;&#xA;&#xD;&#xA;Scénario :&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer les modèles de planning&quot;, par  l’User Action &quot;Créer un modèle de planning&quot;,&#xD;&#xA;Ce scénario concerne la tâche &quot;Créer un modèle de planning&quot;." rationale="Pré-requis :&#xD;&#xA;Le système a présenté à l’utilisateur le formulaire de &quot;Gestion des modèles de planning&quot;.&#xD;&#xA;L'utilisateur demande à créer un modèle de planning.&#xD;&#xA;&#xD;&#xA;" acceptanceCriteria=""/>
        <requirements xmi:id="_zZ__AZs9EeCaWIZ-uoUj9A" name="Modifier un modèle de planning" id="CU02_US12" statement="L'utilisateur veut pouvoir modifier une phase afin de pouvoir mettre à jour un catalogue de modèles de planning disponibles lors de l'initialisation d'un planning prévisionnel.&#xD;&#xA;&#xD;&#xA;Scénario :&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer des modèles de planning&quot;, par  l’User Action &quot;Modifier un modèle de planning&quot;,&#xD;&#xA;Ce scénario concerne les tâches &quot;Modifier une phase un modèle de planning&quot; et &quot;lier  /délier phase dans modèle de planning&quot;" rationale="Pré-requis :&#xD;&#xA;Le système a présenté à l’utilisateur le formulaire de &quot;Gestion des modèles de planning&quot;.&#xD;&#xA;Au moins une phase et un modèle de planning existent en base de données.&#xD;&#xA;L'utilisateur demande à modifier modèle de planning&#xD;&#xA;&#xD;&#xA;" acceptanceCriteria=""/>
        <requirements xmi:id="_zZ__Aps9EeCaWIZ-uoUj9A" name="Consulter un modèle de planning" id="CU02_US13" statement="L'utilisateur veut pouvoir consulter un modèle de planning afin de pouvoir gérer les modèles de planning.&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Scénario :&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer les modèles de planning&quot;, par  l’User Action &quot;Consulter un modèle de planning&quot;,&#xD;&#xA;Ce scénario concerne la tâche &quot;Consulter un modèle de planning&quot;." rationale="Pré-requis :&#xD;&#xA;Le système a présenté à l’utilisateur le formulaire de &quot;Gestion des modèles de planning&quot;.&#xD;&#xA;Au moins un modèle de planning existe en base de données.&#xD;&#xA;L'utilisateur demande à consulter un modèle de planning.&#xD;&#xA;" acceptanceCriteria=""/>
        <requirements xmi:id="_zZ__A5s9EeCaWIZ-uoUj9A" name="Supprimer un modèle de planning" id="CU02_US14" statement="L'utilisateur veut pouvoir supprimer un modèle de planning afin de pouvoir mettre à jour liste des modèles de planning disponible lors de l'initialisation des plannings prévisionnels.&#xD;&#xA;&#xD;&#xA;Scénario :&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer les modèles de planning&quot;, par  l’User Action &quot;Supprimer un modèle de planning&quot;,&#xD;&#xA;Ce scénario concerne la tâche &quot;Supprimer un modèle de planning&quot;." rationale="Pré-requis :&#xD;&#xA;Le système a présenté à l’utilisateur le formulaire de &quot;Gestion des modèle de planning&quot;.&#xD;&#xA;Au moins un modèle de planning existe en base de données.&#xD;&#xA;L'utilisateur demande à supprimer un modèle de planning.&#xD;&#xA;&#xD;&#xA;" acceptanceCriteria=""/>
        <requirements xmi:id="_70t-4FFjEeGfc5rCaC8Ocg" name="Créer une liste de référence" id="CU02_US21" statement="Objectif  &#xD;&#xA;L'administrateur d’un organisme de niveau 2 ou 3 veut créer une liste de référence pour les membres des organismes qui dépendent de lui.&#xD;&#xA;&#xD;&#xA;Pré-requis    &#xD;&#xA;L’utilisateur sélectionne le domaine métier « Administrer COSI » et choisit la ligne « Liste de références » dans le tableau de « choix des éléments ».&#xD;&#xA;&#xD;&#xA;L’utilisateur possède un profil ayant la fonction « créer_modifier_supprimer_affecter_les_listes_de_références » de niveau 2 ou 3.&#xD;&#xA;&#xD;&#xA;Description du user story&#xD;&#xA;Ce scénario débute dans la tâche « Gérer les listes de référence »&#xD;&#xA;Ce scénario concerne les tâches « Gérer les listes de référence » et « Modifier une liste de travail »&#xD;&#xA;"/>
        <requirements xmi:id="_rAXu0FDdEeGO4sDaSbCwZA" name="Modifier une liste de référence" id="CU02_US22" statement="Objectif  &#xD;&#xA;L'administrateur d’un organisme de niveau 2 ou 3 veut modifier une liste de référence existante pour les membres des organismes qui dépendent de lui.&#xD;&#xA;&#xD;&#xA;Pré-requis    &#xD;&#xA;L’utilisateur sélectionne le domaine métier « Administrer COSI » et choisit la ligne « Liste de références » dans le tableau de « choix des éléments ».&#xD;&#xA;&#xD;&#xA;L’utilisateur possède un profil ayant la fonction « créer_modifier_supprimer_affecter_les_listes_de_référence » de niveau 2 ou 3.&#xD;&#xA;&#xD;&#xA;Description du user story&#xD;&#xA;Ce scénario débute dans la tâche « Gérer les listes de références »&#xD;&#xA;Ce scénario concerne les tâches « Gérer les listes de référence » et « Modifier une liste de travail »&#xD;&#xA;"/>
        <requirements xmi:id="_0RNmoFFiEeGfc5rCaC8Ocg" name="Consulter une liste de référence" id="CU02_US23" statement="Objectif  &#xD;&#xA;L'administrateur d’un organisme de niveau 3 veut consulter une liste de référence existante pour les membres des organismes qui dépendent de lui.&#xD;&#xA;&#xD;&#xA;Pré-requis    &#xD;&#xA;L’utilisateur sélectionne le domaine métier « Administrer COSI » et choisit la ligne « Liste de références » dans le tableau de « choix des éléments ».&#xD;&#xA;&#xD;&#xA;L’utilisateur possède un profil ayant la fonction « consulter_les_listes_de_référence » de niveau 2 ou 3.&#xD;&#xA;&#xD;&#xA;Description du user story&#xD;&#xA;Ce scénario débute dans la tâche « Gérer les listes de références »&#xD;&#xA;Ce scénario concerne les tâches « Gérer les listes de référence » et « Modifier une liste de travail »&#xD;&#xA;" status=""/>
        <requirements xmi:id="_u3kp4FDdEeGO4sDaSbCwZA" name="Supprimer une liste de référence" id="CU02_US24" statement="Objectif  &#xD;&#xA;L'administrateur d’un organisme de niveau 2 ou 3 veut supprimer une liste de référence existante pour les membres des organismes qui dépendent de lui.&#xD;&#xA;&#xD;&#xA;Pré-requis    &#xD;&#xA;L’utilisateur sélectionne le domaine métier « Administrer COSI » et choisit la ligne « Liste de références » dans le tableau de « choix des éléments ».&#xD;&#xA;&#xD;&#xA;L’utilisateur possède un profil ayant la fonction « créer_modifier_supprimer_affecter_les_listes_de_référence » de niveau 2 ou 3.&#xD;&#xA;&#xD;&#xA;Description du user story&#xD;&#xA;Ce scénario débute dans la tâche « Gérer les listes de références »&#xD;&#xA;Ce scénario concerne les tâches « Gérer les listes de référence »&#xD;&#xA;"/>
        <requirements xmi:id="_vbU3MFDdEeGO4sDaSbCwZA" name="Affecter une liste de référence" id="CU02_US25" statement="Objectif  &#xD;&#xA;L'administrateur d’un organisme de niveau 2 veut affecter une liste de référence existante pour les membres des organismes de niveau 3 qui dépendent de lui.&#xD;&#xA;&#xD;&#xA;Pré-requis    &#xD;&#xA;L’utilisateur sélectionne le domaine métier « Administrer COSI » et choisit la ligne « Liste de références » dans le tableau de « choix des éléments ».&#xD;&#xA;&#xD;&#xA;L’utilisateur possède un profil ayant la fonction « créer_modifier_supprimer_affecter_les_listes_de_référence » de niveau 2.&#xD;&#xA;&#xD;&#xA;Description du user story&#xD;&#xA;Ce scénario débute dans la tâche « Gérer les listes de références »&#xD;&#xA;Ce scénario concerne les tâches « Gérer les listes de référence » "/>
      </subCategories>
    </subCategories>
    <subCategories xmi:id="_3BuB8JgvEeCFrZJTAzOlZg" name="Abandonnées" id="M02_ABANDON">
      <requirements xmi:id="_gk37QIwXEeC70q5e4kIgCA" name="projet - condition archivage" id="CS_RGF_01_062" statement="L'archivage d'un projet n'est possible ou disponible que si toutes les opérations du projet sont dans l'état &quot;terminé&quot;. (PJ, mémos, informations visualisables)" rationale="" acceptanceCriteria=""/>
      <requirements xmi:id="_JRFlsId3EeCeiKVfaqHkig" name="Phase - Suppression jalons" id="CS_RGF_02_104" statement="Le système doit commencer par supprimer les 2 jalons d'une phase avant suppression de celle ci."/>
      <requirements xmi:id="_XjOrcIVPEeCXi7kGgvvb2w" name="Phase - Suppression phase - notification" id="CS_RGF_02_100" statement="Le système doit notifier chaque utilisateur de planning prévisionnel utilisant cette phase que cette dernière a été supprimée et est donc obsolète par une notification textuelle sous forme d'alerte pour le journal des alertes.">
        
      </requirements>
      <requirements xmi:id="_7TjR8MzREeCPfcn-az1AkQ" name="Phase - initialisation de l'attribut obsolète" id="CS_ABANDON_02_233" statement="A la création de la phase, le champ obsolète est initialisé à faux.&#xD;&#xA;" rationale="" acceptanceCriteria="" subtype="">
        
      </requirements>
      <requirements xmi:id="_3Xed8BEtEeGmf_93g7qDXg" name="Suppression de la fonction" id="CS_ABANDON_02_12_016" statement="La suppression d'une fonction d'un profil ne peut se faire qu'après confirmation de l'utilisateur (&quot;Confirmez-vous la suppression de la fonction pour ce profil ?&quot;)&#xD;&#xA;"/>
    </subCategories>
    <subCategories xmi:id="_8QE9MJgvEeCFrZJTAzOlZg" name="Mise de coté" id="M02_MDC">
      <requirements xmi:id="_ys778Id5EeCeiKVfaqHkig" name="CRUD phases - filtre catalogue" id="CS_RGF_02_130" statement="Le système doit filtrer la liste des phases par catalogue ou par aucun catalogue choisi" acceptanceCriteria=""/>
      <requirements xmi:id="_ys778Yd5EeCeiKVfaqHkig" name="CRUD phases - choix catalogue valeur TOUS" id="CS_RGF_02_131" statement="Le système doit présenter par défaut la valeur &quot;tous catalogue&quot; à l'utilisateur et donc présenter les phases paginées par 10." rationale=""/>
      <subCategories xmi:id="_SyPK0IeCEeCeiKVfaqHkig" name="CRUD catalogue de phases" id="M02_MDC_01">
        <requirements xmi:id="_SyPK0YeCEeCeiKVfaqHkig" name="Catalogue de phases - Règle de présentation du catalogue de phase" id="CS_RGF_02_140" statement="Le système doit présenter le catalogue de phases trié par ordre alphabétique croissant du libellé de catalogue." acceptanceCriteria="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_KrPOgIeCEeCeiKVfaqHkig" name="Créer un catalogue" id="M02_MDC_02">
        <requirements xmi:id="_KrPOgYeCEeCeiKVfaqHkig" name="Catalogue de phases - création catalogue" id="CS_RGF_02_141" statement="Le système doit vérifier qu'aucun autre catalogue de phases ne porte le même libellé ou sortir en erreur.">
          
          
        </requirements>
        <requirements xmi:id="_KrPOgoeCEeCeiKVfaqHkig" name="Catalogue de phases - Msg erreur création catalogue" id="CS_RGF_02_142" statement="Si le système ne peut créer de catalogue pour raison du libellé existant, il doit proposer le message suivant dans une &quot;oui&quot; box :&#xD;&#xA;&quot;Un autre catalogue porte déjà ce libellé, veuillez le modifier&quot;">
          
          
          
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_KrPOhYeCEeCeiKVfaqHkig" name="Modifier un catalogue" id="M02_MDC_03">
        <requirements xmi:id="_KrPOiIeCEeCeiKVfaqHkig" name="Catalogue de phases - consultation" id="CS_RGF_02_143" statement="Si un consultation est demandée, l'écran de modification de catalogue est en lecture seule.">
          
        </requirements>
        <requirements xmi:id="_zIgSEIeDEeCeiKVfaqHkig" name="Catalogue de phases - présentation de la liste des phases" id="CS_RGF_02_145" statement="Le système doit présenter la liste des phases dans un tableau ordonné par ordre croissant sur le libellé de phase, puis sur le code de la phase." rationale="">
          
          
          
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_KrPOiYeCEeCeiKVfaqHkig" name="Supprimer un catalogue" id="M02_MDC_04">
        <requirements xmi:id="_KrPOjIeCEeCeiKVfaqHkig" name="Catalogue de phases - Msg Suppression catalogue" id="CS_RGF_02_144" statement="Le système doit demander une confirmation de suppression d'un catalogue ; le message présenté à l'utilisateur dans ce cadre est :&#xD;&#xA;&quot;Voulez-vous vraiment supprimer ce catalogue de phases ?&quot;." rationale="">
          
        </requirements>
      </subCategories>
    </subCategories>
    <subCategories xmi:id="_IBZ5AA-LEeGG9cEVh9B7ZA" name="CU12 - Gérer les droits" id="">
      <requirements xmi:id="_QDKdYB9MEeGtt_8fYalctQ" name="Droit : Cumul des profils" id="CS_RGF_02_12_155" statement="Si un utilisateur est associé à plusieurs profil, l'utilisateur dispose les droits de l'ensemble des fonctions de chaque profils.">
        
      </requirements>
      <requirements xmi:id="_gm-1AB9MEeGtt_8fYalctQ" name="Droit : Cumul de profil et niveau de fonction" id="CS_RGF_02_12_156" statement="Si un utilisateur est attaché à plusieurs profils et si certains référencent une même fonction avec des niveaux différents, on considère que l'utilisateur à le droit sur la fonction avec le niveau le plus fort (hiérarchiquement) enregistré dans les profils. ">
        
      </requirements>
      <subCategories xmi:id="_Ugv-oB8uEeGtt_8fYalctQ" name="Navigateur" id="CU12_01">
        <requirements xmi:id="_uIAQcBwfEeGPP9xbskrd2A" name="Droits : Administrer les droits" id="CS_RGF_02_12_100" statement="Seuls les utilisateurs associés à un profil ayant la fonction &quot;consulter_les_profils&quot; peuvent accéder à cette fonctionnalité" rationale="">
          
          
        </requirements>
        <requirements xmi:id="_KuaQMBwgEeGPP9xbskrd2A" name="Navigateur : Affichage de l'arborescence de travail" id="CS_RGF_02_12_101" statement="L'arborescence affichée est constituée de la liste des utilisateurs.&#xD;&#xA;Elle est organisée comme suit : &#xD;&#xA;Organisme (libellé abrégé) &#xD;&#xA;Type de profil (Profil générique, profil spécifique, poste responsable) &#xD;&#xA;Profil (libellé) &#xD;&#xA;" rationale="">
          
          
        </requirements>
        <requirements xmi:id="_mQWs0BwhEeGPP9xbskrd2A" name="Navigateur : Choix d'affichage : Organisme" id="CS_RGF_02_12_102" statement="L'utilisateur peut choisir que la liste des utilisateurs soit affichée selon l'organisme d'appartenance des utilisateurs ou non. Dans ce cas, l'arborescence sera organisée pour afficher un nœud supplémentaire composé du liba de l'organisme">
          
          
        </requirements>
        <requirements xmi:id="_6GzS0BwhEeGPP9xbskrd2A" name="Navigateur : Choix d'affichage : Fonction" id="CS_RGF_02_12_103" statement="L'utilisateur a le choix d'afficher la liste des profils triée selon les fonctions qu’ils possèdent. Dans ce cas l’arborescence sera organisée pour afficher un nœud supplémentaire composé du nom de la fonction  sous le nœud de type de profil spécifique et générique">
          
          
        </requirements>
        <requirements xmi:id="_GexVEBwiEeGPP9xbskrd2A" name="Navigateur : Choix d'affichage : Profil" id="CS_RGF_02_12_104" statement="L'utilisateur a le choix de trier la liste selon le profil d'appartenance des utilisateurs (par défaut) ou non. Un nœud composé du libellé du profil sera ajouté par profil à l’arborescence">
          
          
        </requirements>
        <requirements xmi:id="_Vj5hIBwiEeGPP9xbskrd2A" name="Navigateur : Choix d'affichage : Utilisateur" id="CS_RGF_02_12_105" statement="L'utilisateur a le choix d’afficher les utilisateurs (non cochée par défaut). Il ne peut cependant cocher à la fois les utilisateurs et les fonctions">
          
          
        </requirements>
        <requirements xmi:id="_10xyEBwjEeGPP9xbskrd2A" name="Navigateur : Filtre sur l'organisme de l'utilisateur" id="CS_RGF_02_12_106" statement="Via une case à cocher, l'utilisateur peut demander à filtrer la liste aux utilisateurs qui sont uniquement de son organisme et de ceux des organismes de niveau inférieurs qui lui sont rattaché (affichage par défaut).&#xD;&#xA;&#xD;&#xA;Sinon, la liste des utilisateurs est déterminée par le niveau associé à la fonction &quot;Administrer les droits&quot; au profil utilisé par l'utilisateur. &#xD;&#xA;(si le niveau est 1, tous les utilisateurs seront affichés)&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_D7KzEBwmEeGPP9xbskrd2A" name="Navigateur : Filtre par attribut utilisateur" id="CS_RGF_ABANDON_02_12_107" statement="L'utilisateur peut filtrer la liste selon un attribut utilisateur (nom, prenom, email, grade, login, fonction, ville, entité, téléphone, fax, service, email internet, bâtiment) et un critère saisi. L'attribut choisi devient le premier niveau de l'arborescence affiché" rationale=""/>
        <requirements xmi:id="_8dgYkBwuEeGPP9xbskrd2A" name="Navigateur : Affichage des barres d'outils" id="CS_RGF_02_12_108" statement="L'écran affiche quatre barres d'outils :&#xD;&#xA;- Profil générique&#xD;&#xA;- Profi spécifique&#xD;&#xA;- Poste responsable&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_L3faUBwvEeGPP9xbskrd2A" name="Navigateur : Barre outil profil générique" id="CS_RGF_02_12_109" statement="La barre d'outils profil générique propose les outils suivants :&#xD;&#xA; - Créer un profils générique (disponible sur sélection du noeud &quot;profils génériques&quot;)&#xD;&#xA; - Modifier un profil générique (disponible sur sélection d'un profil générique)&#xD;&#xA; - Consulter un profil générique (disponible sur sélection d'un profil générique)&#xD;&#xA; - Supprimer un profil générique (disponible sur sélection d'un profil générique)&#xD;&#xA; - Associer des utilisateurs (disponible sur sélection d'un profil générique)">
          
        </requirements>
        <requirements xmi:id="_4KqN4BwvEeGPP9xbskrd2A" name="Navigateur : Barre outil profil spécifique" id="CS_RGF_02_12_110" statement="La barre d’outils profil spécifique propose l'outil : &#xD;&#xA;- Créer un profil spécifique (disponible sur sélection du noeud &quot;profils spécifique s&quot;)&#xD;&#xA; - Modifier un profil spécifique (disponible sur sélection d'un profil spécifique)&#xD;&#xA; - Supprimer un profil spécifique (disponible sur sélection d'un profil spécifique)&#xD;&#xA;- Consulter un profil spécifique (disponible sur sélection d'un profil spécifique)&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_JLBXkBwwEeGPP9xbskrd2A" name="Navigateur : Barre outil poste responsable" id="CS_RGF_02_12_111" statement="La barre d'outil poste responsable propose les outils suivants :&#xD;&#xA;- Créer un poste responsable (disponible sur sélection du noeud type de profil poste responsable)&#xD;&#xA;- Modifier un poste responsable (disponible sur sélection d'un poste responsable)&#xD;&#xA;- Supprimer un poste responsable (disponible sur sélection d'un poste responsable)&#xD;&#xA;- Consulter un poste responsable (disponible sur sélection d'un poste responsable)">
          
        </requirements>
        <requirements xmi:id="_XuaV0B8gEeGtt_8fYalctQ" name="Charger la liste des attributs utilisateur" id="CS_RGF_02_12_112" statement="Charger la liste des attributs utilisateur pour les critères de tri">
          
        </requirements>
        <requirements xmi:id="_HrticBEjEeGmf_93g7qDXg" name="Suppression des profils" id="CS_RGF_02_12_007" statement="Seuls les profils sans utilisateurs peuvent être supprimés" rationale="" status="">
          
          
          
        </requirements>
        <requirements xmi:id="_o_E1gBwkEeGPP9xbskrd2A" name="Charger la liste des utilisateurs" id="CS_RGF_02_12_113" statement="Charger la liste des utilisateurs selon le niveau attaché à la fonction &quot;Administrer les droits&quot; et l'organisme de l'utilisateur. &#xD;&#xA;Exemple : Si le niveau spécifié est 1, charger la liste de tous les utilisateurs&#xD;&#xA;Si le niveau spécifié est 2, si l'utilisateur à comme organisme de niveau 2 &quot;ESID IDF&quot;, tous les utilisateurs d'ile de France seront chargé (ESID + USID)" rationale="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_97j6MBEmEeGmf_93g7qDXg" name="Suppression de profil" id="CU12_02">
        <requirements xmi:id="_j8yLYBEjEeGmf_93g7qDXg" name="Confirmation de suppression" id="CS_RGF_02_12_006" statement="La suppression d'un profil ne peut se faire qu'après confirmation de l'utilisateur (&quot;Confirmez-vous la suppression de ce profil ?&quot;)">
          
          
        </requirements>
        <requirements xmi:id="_tAizMBa7EeGgAtcpIUkjng" name="Suppression de profil" id="CS_RGF_02_12_042" statement="Le profil non associé a un utilisateur est supprimé physiquement">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_qEvEQBEnEeGmf_93g7qDXg" name="Créer - Modifier un profil" id="CU12_03 ">
        <requirements xmi:id="_yyiesBEjEeGmf_93g7qDXg" name="Charger les informations du profils" id="CS_RGF_02_12_005" statement="Pour le profil sélectionné, les informations à charger sont :&#xD;&#xA;Libellé du profil&#xD;&#xA;Type du profil (Générique ou spécifique)&#xD;&#xA;La liste des fonctions attachées à ce profil (libellé, commentaire, niveau d'organisme)&#xD;&#xA;">
          
          
          
        </requirements>
        <requirements xmi:id="_0oCq8BEnEeGmf_93g7qDXg" name="Afficher l'écran des paramètres du profil" id="CS_RGF_02_12_010" statement="Afficher dans un onglet les éléments d'identification du profil : &#xD;&#xA;Libellé (obligatoire)&#xD;&#xA;Libellé court&#xD;&#xA;Complémentaire ou non (uniquement pour les profils spécifiques en fonction du rôle qui leur est associé)&#xD;&#xA;Commentaire&#xD;&#xA;&#xD;&#xA;Dans le cas d'une modification, les champs sont initialisés en fonction des informations du profil sélectionné.&#xD;&#xA;&#xD;&#xA;Dans le cas d'un profil spécifique, les champs sont non modifiables&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;" status="">
          
        </requirements>
        <requirements xmi:id="_oBtAEBEoEeGmf_93g7qDXg" name="Libellé obligatoire" id="CS_RGF_02_12_011" statement="Le libellé d'un profil est obligatoire">
          
          
        </requirements>
        <requirements xmi:id="_ZlJacBEpEeGmf_93g7qDXg" name="Modification du type de profil" id="CS_RGF_02_12_012" statement="Le type de profil n'est pas modifiable">
          
          
          
        </requirements>
        <requirements xmi:id="_eRlhEBH9EeGp44SaM1hlIg" name="Création de profil " id="CS_RGF_02_12_041" statement="Le profil générique ou spécifique est persisté " rationale="">
          
        </requirements>
        <requirements xmi:id="__r98wB9cEeGtt_8fYalctQ" name="Droit : Créer / modifier profil" id="CS_RGF_02_12_120" statement="Seuls les utilisateurs ayant au moins un profil avec la fonction &quot;creer_modifier_supprimer_les_profils&quot; peuvent accéder à cette tâche">
          
          
          
          
        </requirements>
        <requirements xmi:id="_lL13MCGEEeGgE_Dx16kb1Q" name="Profil spécifique existant pour un organisme et un rôle" id="CS_RGF_02_12_056" statement="Si il existe déjà un profil spécifique de même rôle pour l'organisme, l'enregistrement est impossible" rationale="">
          
        </requirements>
        <requirements xmi:id="_3Y8g0CGEEeGgE_Dx16kb1Q" name="Organisme non spécifié" id="CS_RGF_02_12_057" statement="Si l'utilisateur n'a pas spécifié d'organisme, le profil est associé à l'organisme de l'utilisateur lors de la création.&#xD;&#xA;En modification, l'organisme n'est pas modifié.&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_tbotsCGFEeGgE_Dx16kb1Q" name="LOV organisme" id="CS_RGF_02_12_059" statement="LOV - organisme associé au profil&#xD;&#xA;- Sélection simple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Restriction:&#xD;&#xA;Présenter uniquement les organismes du SID, c'est-à-dire les USID, ESID, DCSID, DID ayant un niveau de 1 à 3&#xD;&#xA;Résultat :&#xD;&#xA;- trié par libellé abrégé croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- id organisme (RF_ORGANISME_ID)&#xD;&#xA;- libellé abrégé (RF_ORGANISME_LIBA)&#xD;&#xA;- Type organisme (RF_TORG_LIB)&#xD;&#xA;Champs retournés :&#xD;&#xA;- code organisme (RF_ORGANISME_COD)&#xD;&#xA;-  libellé abrégé de l'organisme (RF_ORGANISME_LIBA)">
          
          
        </requirements>
        <requirements xmi:id="_4MqnUCGFEeGgE_Dx16kb1Q" name="Disponiblité de Choisir un organisme" id="CS_RGF_02_12_058" statement="Le choix de l'organisme n'est possible que si la fonction &quot;Creer_Modifier_Supprimer_les_profils&quot; du profil de l'utilisateur est associée à un niveau 1.Uniquement en création">
          
          
        </requirements>
        <requirements xmi:id="_pxSJsDrdEeGxhPBr_GwUDQ" name="Choix du rôle d'un profil spécifique" id="CS_RGF_02_12_501" statement="Les profils spécifiques et seuls les profils spécifiques doivent obligatoirement être associés à un rôle." rationale="">
          
        </requirements>
        <requirements xmi:id="_GJgEEEZxEeGIB7OgXFna0A" name="Obsolescence d'un profil spécifique" id="CS_RGF_02_12_300" statement="Seuls les profils spécifiques en modification peuvent être identifié comme obsolète si il existe au moins un profil spécifique se même rôle pour un organisme de niveau inférieur. (exemple : On ne pourra rendre obsolète qu'un profil spécifique d'un USID si il existe un profil spécifique de même rôle pour l'ESID père ou le SID)" rationale="">
          
        </requirements>
        <requirements xmi:id="_MRrt0EZyEeGIB7OgXFna0A" name="Modification du type de rôle" id="CS_RGF_02_12_710" statement="En modification, le type de rôle est affiché mais n'est pas modifiable" rationale="">
          
        </requirements>
        <requirements xmi:id="_8pKGoFiBEeGF7fIC5Pud7A" name="Contenu détail sur création modification de profil" id="CS_RGF_02_12_160" statement="La zone de détail contient :&#xD;&#xA;Si création de profil : Rien&#xD;&#xA;Si modification de profil : le libellé du profil, son libellé court et son commentaire&#xD;&#xA;" rationale="">
          
          
        </requirements>
        <requirements xmi:id="_m0ywgFiDEeGF7fIC5Pud7A" name="Unicité des libellés des profils" id="CS_RGF_02_12_500" statement="Un profil ne peut pas être créé si un autre profil (de même type) porte le même libellé pour un même organisme. Afficher un message d’erreur invitant l’utilisateur à modifier le libellé du profil" rationale="">
          
        </requirements>
        <requirements xmi:id="_Ras5IGLZEeGyvYWyYmXL5Q" name="Profil par défaut : Initialisation application" id="CS_RGF_02_12_401" statement="A l'initialisation de l'application, un profil par défaut doit être créé pour chaque organisme sans aucun rôle. Par défaut il n’est pas associé aux nouveaux utilisateurs :&#xD;&#xA;Libellé : Profil + ibellé abrégé org&#xD;&#xA;Profil par défaut : Oui&#xD;&#xA;Associer au utilisateur : Non&#xD;&#xA;&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_Hwlu0GLaEeGyvYWyYmXL5Q" name="Profil par défaut : Changement" id="CS_RGF_02_12_402" statement="Un profil typé &quot;profil par défaut&quot; peut être désactivé pour ne pas être automatiquement associé aux nouveaux utilisateurs sauf si il est de niveau 1 " rationale="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_5EVIsBEqEeGmf_93g7qDXg" name="Associer les fonctions" id="CU12_04">
        <requirements xmi:id="_C54RgBErEeGmf_93g7qDXg" name="Charger la liste des fonctions" id="CS_RGF_02_12_014" statement="La liste des fonctions associées au profil est chargée avec :&#xD;&#xA;- Libellé de la fonction&#xD;&#xA;- Type de la fonction&#xD;&#xA;- Commentaire de la fonction&#xD;&#xA;- le niveau de la contrainte (1 à 3)&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_Mfoq0BErEeGmf_93g7qDXg" name="Liste des fonctions" id="CS_RGF_02_12_015" statement="La liste des fonctions du profil sont affichées dans un tableau multi sélection, trié selon le type puis le code de la fonction :&#xD;&#xA;- Libellé de la fonction&#xD;&#xA;- Type de la fonction&#xD;&#xA;- Description associé à la fonction&#xD;&#xA;- Niveau de la contrainte&#xD;&#xA;" status="">
          
        </requirements>
        <requirements xmi:id="_XwH1sBEtEeGmf_93g7qDXg" name="LOV fonctions" id="CS_RGF_02_12_016" statement="LOV fonction&#xD;&#xA;- Sélection multiple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Résultat :&#xD;&#xA;- trié par  type puis libellé de la fonction croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- libellé de la fonction&#xD;&#xA;- commentaire de la fonction&#xD;&#xA;- type de la fonction&#xD;&#xA;&#xD;&#xA;Champs affichés :&#xD;&#xA;- Libellé de la fonction&#xD;&#xA;- Type de la fonction&#xD;&#xA;- Fonction mère&#xD;&#xA;- Commentaire de la fonction&#xD;&#xA;Champs retournés :&#xD;&#xA;id de la fonction&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_JRelYBEuEeGmf_93g7qDXg" name="Associer les fonctions au profil" id="CS_RGF_02_12_017" statement="La liste des fonctions et la configuration de la contrainte géographique est associé au profil.">
          
        </requirements>
        <requirements xmi:id="_HafUsBa3EeGgAtcpIUkjng" name="Choisir le niveau de contrainte" id="CS_RGF_02_12_051" statement="Le niveau de contrainte est à choisir entre 1 à 3 uniquement pour un profil de type générique. On ne spécifie pas de niveau pour un profil spécifique">
          
        </requirements>
        <requirements xmi:id="_er9Z8B8kEeGtt_8fYalctQ" name="Fonction de type Profil" id="CS_RGF_02_12_114" statement="Dans le cas de l'association avec un profil spécifique, ne pas charger la liste des fonctions destinées à la manipulation de profils (dont le type est &quot;profil&quot;)">
          
        </requirements>
        <requirements xmi:id="__pk-cCF9EeGgE_Dx16kb1Q" name="Vérifier la dépendance entre fonction" id="CS_RGF_02_12_050" statement="Si une fonction fille est sélectionnée dans la liste alors que sa mère n'est pas présente, ajouter automatiquement la fonction mère">
          
          
        </requirements>
        <requirements xmi:id="_drFi4CGBEeGgE_Dx16kb1Q" name="Contrainte niveau mère" id="CS_RGF_02_12_052" statement="Une fonction dépendante d'une fonction mère ne peut se voir associer un niveau inférieur au niveau associé à sa fonction mère. &#xD;&#xA;Exemple : Une fonction fille ne peut se voir attribuer un niveau 1 si sa fonction mère est attribuée à un niveau 2">
          
        </requirements>
        <requirements xmi:id="_dpGwcCGCEeGgE_Dx16kb1Q" name="Message d'erreur : niveau non conforme" id="CS_RGF_02_12_053" statement="Si le niveau d'une fonction fille est inférieur au niveau d'une fonction mère, indiquer à l'utilisateur : &quot;Pour la fonction FCT_FILLE, vous ne pouvez associer un niveau hiérarchique plus élevé que celui de la fonction FCT_MERE&quot;">
          
        </requirements>
        <requirements xmi:id="_0iKy4CGCEeGgE_Dx16kb1Q" name="Message d'erreur : Fonction mère non présente" id="CS_RGF_02_12_054" statement="Si une fonction fille est présente dans la liste sans sa fonction mère, afficher à l'utilisateur : &quot;La fonction FCT_FILLE ne peut être ajoutée sans la fonction FCT_MERE&quot;">
          
        </requirements>
        <requirements xmi:id="_-Rt-kEtJEeGi3u6K18qggQ" name="Vérifier la suppression de fonctions filles" id="CS_RGF_02_12_055" statement="La suppression d’une fonction mère provoque une demande de confirmation listant les fonctions filles qui seront supprimées du profil. L’acceptation provoque la suppression de la fonction mère et de ses fonctions filles du profil, sinon aucune fonction n’est supprimée du profil.">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_dlssgBEuEeGmf_93g7qDXg" name="Associer les utilisateurs" id="CU12_05">
        <requirements xmi:id="_bqDCsBEvEeGmf_93g7qDXg" name="Charger la liste des utilisateurs" id="CS_RGF_02_12_018" statement="Le système charge la liste des utilisateurs associés au profil avec les informations suivantes :&#xD;&#xA;Nom&#xD;&#xA;Prénom&#xD;&#xA;Grade&#xD;&#xA;Login&#xD;&#xA;email&#xD;&#xA;Organisme (liba)">
          
          
        </requirements>
        <requirements xmi:id="_wAILABEvEeGmf_93g7qDXg" name="Afficher la liste des utilisateurs" id="CS_RGF_02_12_022" statement="La liste des utilisateurs associés au profil est affichée avec les informations suivantes, triées selon le nom et le prénom :&#xD;&#xA;- Nom&#xD;&#xA;- Prénom&#xD;&#xA;- Login&#xD;&#xA;- Email&#xD;&#xA;- Organisme (liba)&#xD;&#xA;">
          
          
        </requirements>
        <requirements xmi:id="_OKpo4BEwEeGmf_93g7qDXg" name="LOV utilisateur" id="CS_RGF_02_12_019" statement="LOV utilisateur&#xD;&#xA;- Sélection multiple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;- Tous les utilisateurs (non déjà rattaché au profil) du même organisme ou d'un organisme de hiérarchie inférieure à celui de l'utilisateur &#xD;&#xA;Résultat :&#xD;&#xA;- trié par nom et prénom croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- Nom&#xD;&#xA;- prénom&#xD;&#xA;- Email&#xD;&#xA;- login&#xD;&#xA;- Organisme(liba)&#xD;&#xA;&#xD;&#xA;Champs affichés :&#xD;&#xA;- Nom&#xD;&#xA;- Prénom&#xD;&#xA;- Login&#xD;&#xA;- Email&#xD;&#xA;- Organisme&#xD;&#xA;&#xD;&#xA;Champs retournés :&#xD;&#xA;id de l'utilisateur&#xD;&#xA;" rationale="">
          
          
        </requirements>
        <requirements xmi:id="_dZ3voBExEeGmf_93g7qDXg" name="Suppression d'un utilisateur" id="CS_RGF_02_12_020" statement="La suppression d'un profil pour un utilisateur ne peut se faire que suite à une confirmation de l'utilisateur (&quot;Confirmez-vous la suppression du profil pour l'utilisateur ?&quot;)&#xD;&#xA;">
          
          
        </requirements>
        <requirements xmi:id="_-TebMBE3EeGmf_93g7qDXg" name="Enregistrement du profil pour l'utilisateur" id="CS_RGF_02_12_021" statement="L'enregistrement provoque :&#xD;&#xA;- Pour les utilisateurs ajoutés :&#xD;&#xA;   - L'ajout du profil pour les utilisateurs. &#xD;&#xA;- Pour les utilisateurs supprimés du profil :&#xD;&#xA;   - La suppression du lien avec le profil et la perte des droits sur les fonctions lors de la prochaine connexion&#xD;&#xA;- Pour le profil : La mise à jour de la liste des utilisateurs qui lui sont affectés&#xD;&#xA;">
          
          
        </requirements>
        <requirements xmi:id="_zZi80BH-EeGp44SaM1hlIg" name="Associer des utilisateurs  à un profil générique" id="CS_RGF_02_12_023" statement="L'association du profil avec les utilisateurs ne peut se faire que si le profil est de type générique">
          
          
        </requirements>
        <requirements xmi:id="_WDeYAB9iEeGtt_8fYalctQ" name="Droit : Associer des utilisateurs" id="CS_RGF_02_12_121" statement="Seuls les utilisateurs ayant au moins un profil avec la fonction &quot;associer_des_utilisateur_a_un profil&quot; peuvent accéder à cette tâche" rationale="" status="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_CwEkQBRXEeGTEuEVVJ_8xw" name="Gérer les postes responsables" id="CU12_06">
        <requirements xmi:id="_QaHSYBRXEeGTEuEVVJ_8xw" name="Charger la liste des postes responsables" id="CS_RGF_02_12_030" statement="Le système charge la liste des postes responsables de l'organisme de l'utilisateur ou de ses niveaux inférieurs avec les informations suivantes :&#xD;&#xA;- Libellé du poste&#xD;&#xA;- Le nom et prénom de l'utilisateur associé au poste" rationale="">
          
          
        </requirements>
        <requirements xmi:id="_FG1x4BRYEeGTEuEVVJ_8xw" name="Suppression de poste non utilisé" id="CS_RGF_02_12_032" statement="La suppression d'un poste responsable n'est possible que si ce poste n'est pas utilisé pour organiser une opération">
          
          
        </requirements>
        <requirements xmi:id="_OD820BRYEeGTEuEVVJ_8xw" name="Confirmation avant suppression" id="CS_RGF_02_12_033" statement="Un utilisateur ne peut supprimer un poste responsable sans confirmation">
          
          
        </requirements>
        <requirements xmi:id="_hfIdIBRYEeGTEuEVVJ_8xw" name="Suppression d'un poste" id="CS_RGF_02_12_034" statement="Le poste responsable est supprimé physiquement">
          
          
        </requirements>
        <requirements xmi:id="__VEzwDYmEeGvzOEgWZw0OQ" name="Modifier un poste responsable" id="CS_RGF_02_12_040" statement="La modification d'un utilisateur de poste responsable provoque l'historisation du couple poste responsable - utilisateur avec une date de fin initialisée à la date du jour et la création d'un nouveau poste responsable pour le nouvel utilisateur">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_HIFtYBRXEeGTEuEVVJ_8xw" name="Créer - Modifier un poste responsable" id="CU12_07">
        <requirements xmi:id="_peC_ABRZEeGTEuEVVJ_8xw" name="Charger les informations de l'utilisateur attaché au poste" id="CS_RGF_02_12_035" statement="Si le poste est déjà associé à un utilisateur alors charger :&#xD;&#xA;- Le nom de l'utilisateur&#xD;&#xA;- Le prénom de l'utilisateur&#xD;&#xA;- Le grade&#xD;&#xA;- Le login de l'utilisateur&#xD;&#xA;- L'email de l'utilisateur&#xD;&#xA;- L'organisme de l'utilisateur&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_8nDQMBRZEeGTEuEVVJ_8xw" name="Afficher les informations du poste" id="CS_RGF_02_12_036" statement="L'écran affiche les informations du poste responsable :&#xD;&#xA;- Le libellé du poste responsable&#xD;&#xA;- Le login, le nom, le prénom, le grade, la fonction, l'email  et l'organisme de l'utilisateur associé">
          
        </requirements>
        <requirements xmi:id="_QHJywBRaEeGTEuEVVJ_8xw" name="Choisir un utilisateur" id="CS_RGF_02_12_037" statement="LOV utilisateur&#xD;&#xA;- Sélection simple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;- Tous les utilisateurs  du même organisme ou d'un organisme de hiérarchie inférieure à celui de l'utilisateur &#xD;&#xA;Résultat :&#xD;&#xA;- trié par nom croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- Nom&#xD;&#xA;- prénom&#xD;&#xA;- Email&#xD;&#xA;- login&#xD;&#xA;- Organisme&#xD;&#xA;- Profils&#xD;&#xA;&#xD;&#xA;- Filtre par défaut : Organisme de niveau 2 de l'utilisateur connecté&#xD;&#xA;&#xD;&#xA;Champs affichés :&#xD;&#xA;- Nom&#xD;&#xA;- Prénom&#xD;&#xA;- Login&#xD;&#xA;- Email&#xD;&#xA;- Organisme&#xD;&#xA;- Grade&#xD;&#xA;- Fonction&#xD;&#xA;- Liste des profils (triés par ordre alphabétique)&#xD;&#xA;&#xD;&#xA;Champs retournés :&#xD;&#xA;id de l'utilisateur&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_fdaLEBRaEeGTEuEVVJ_8xw" name="Valider " id="CS_RGF_02_12_038" statement="Pour renseigner un poste responsable le libellé et l'utilisateur sont obligatoires." status="">
          
        </requirements>
        <requirements xmi:id="_onAz4BRaEeGTEuEVVJ_8xw" name="Enregistrer le poste responsable" id="CS_RGF_02_12_039" statement="L'enregistrement consiste à persister le poste responsable et le lien vers l'utilisateur choisi. Le poste responsable est créé avec un lien vers l’organisme de l’utilisateur">
          
        </requirements>
        <requirements xmi:id="_d72noFiIEeGF7fIC5Pud7A" name="Unicité du libellé d'un poste responsable" id="CS_RGF_02_12_399" statement="Un poste responsable ne peut pas être créé si un autre poste responsable porte le même libellé pour un même organisme. Afficher un message d’erreur invitant l’utilisateur à modifier le libellé du poste responsable." rationale="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_mlt9wB8fEeGtt_8fYalctQ" name="Consulter un profil" id="CU12_08">
        <requirements xmi:id="_qu01QB8sEeGtt_8fYalctQ" name="Afficher les informations du profil" id="CS_RGF_02_12_116" statement="Afficher dans un onglet les informations non modifiables du profil : &#xD;&#xA;Libellé&#xD;&#xA;Libellé court&#xD;&#xA;Commentaire&#xD;&#xA;Liste des fonction non modifiables (Libellé, type, commentaire, niveau)">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_icftgB8-EeGtt_8fYalctQ" name="Calcul du périmètre géographique" id="CU12_09">
        <requirements xmi:id="_rBsBsB8-EeGtt_8fYalctQ" name="Droit : La hiérarchie d'organisme de l'utilisateur" id="CS_RGF_02_12_150" statement="La hiérarchie d'organisme de l'utilisateur est déterminée à partir de son organisme et des organismes pères.&#xD;&#xA;&#xD;&#xA;Exemple : &#xD;&#xA;Organisme de l'utilisateur :&#xD;&#xA;USID_CRL &#xD;&#xA;Hiérarchie d'organisme de l’utilisateur &#xD;&#xA;DCSID / ESID_IDF / USID_CRL&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_g7f_YB8_EeGtt_8fYalctQ" name="Droit : Organisme de l'utilisateur" id="CS_RGF_02_12_151" statement="L'utilisateur connecté et authentifié est obligatoirement associé à un organisme">
          
        </requirements>
        <requirements xmi:id="_t80goB8_EeGtt_8fYalctQ" name="Droit : Niveau hiérarchique d'un organisme" id="CS_RGF_02_12_152" statement="Chaque organisme est associé à un niveau correspondant à son niveau hiérarchique (1 pour le plus important hiérarchiquement, 2 et 3).&#xD;&#xA;&#xD;&#xA;Chaque organisme ne peut avoir qu'un organisme père. &#xD;&#xA;Il ne peut y avoir qu'un organisme de niveau 1">
          
        </requirements>
        <requirements xmi:id="_Ma_TQB9AEeGtt_8fYalctQ" name="Droit : Niveau hiérarchique d'un utilisateur" id="CS_RGF_02_12_153" statement="Un utilisateur connecté et authentifié a comme niveau hiérarchique celui de l'organisme auquel il est attaché.">
          
        </requirements>
        <requirements xmi:id="_m5ub4B9AEeGtt_8fYalctQ" name="Droit : Calcul du périmètre géographique" id="CS_RGF_02_12_154" statement="Pour calculer le périmètre géographique d'un utilisateur il faut déterminer la hiérarchie d'organisme de l'utilisateur (à partir de l'organisme de l'utilisateur jusqu'au dernier organisme père).&#xD;&#xA;&#xD;&#xA;Pour une fonction donnée, associée au profil de l'utilisateur avec un niveau n.&#xD;&#xA;Le périmètre géographique de l'utilisateur est l'ensemble des organismes constitués de l'organisme de niveau n dans la hiérarchie de l'utilisateur et de tous ces organismes fils.&#xD;&#xA;">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_z2MJgBEiEeGmf_93g7qDXg" name="ABANDON " id="CU12_00">
        <requirements xmi:id="_FtlQkBEdEeGmf_93g7qDXg" name="Affichage de l'écran" id="CS_ABANDON_02_12_003" statement="L'écran de gestion des profils affiche :&#xD;&#xA;- Afficher le bloc d'identité du profil&#xD;&#xA;- Afficher la liste des fonctions associées au profil&#xD;&#xA;- Afficher la liste des utilisateurs associés au profils&#xD;&#xA;- Afficher les liste des opérations liées au profils" rationale="">
          
        </requirements>
        <requirements xmi:id="_E1vNIBEeEeGmf_93g7qDXg" name="Sélection d'un profil" id="CS_ABANDON_02_12_004" statement="LOV Profil&#xD;&#xA;- Sélection simple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Résultat :&#xD;&#xA;- trié par libellé de profil croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- Libellé du profil&#xD;&#xA;&#xD;&#xA;Champs affichés :&#xD;&#xA;- Libellé du profil&#xD;&#xA;- Est un profil d'organisation Oui / Non&#xD;&#xA;Champs retournés :&#xD;&#xA;id du profil + lilbellé du profil&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_Fb60oBEkEeGmf_93g7qDXg" name="Charger la liste des profils" id="CS_ABANDON_02_12_008" statement="La liste des profils filtrée selon l’organisme de l'utilisateur et le niveau associé à la fonction de consultation des profils est chargée avec les informations suivantes :&#xD;&#xA;Libellé court du profil&#xD;&#xA;Libellé long du profil&#xD;&#xA;Type du profil (générique / spécifique)&#xD;&#xA;Commentaire du profil.&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_9N7qEBEkEeGmf_93g7qDXg" name="Affichage de la liste des profils" id="CS_ABANDON_02_12_009" statement="Les profils sont affichés dans une liste de sélection simple proposant les colonnes suivantes :&#xD;&#xA;- Libellé court du profil&#xD;&#xA;- Libellé long du profil&#xD;&#xA;- Type du profil (spécifique / générique)&#xD;&#xA;&#xD;&#xA;Elle est classée selon le type du profil puis le libellé long.&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_sqi-ABEpEeGmf_93g7qDXg" name="Gestion des instances" id="CS_ABANDON_02_12_013" statement="La gestion des instances n'est disponible que pour les profils de type spécifiques">
          
        </requirements>
        <requirements xmi:id="_8VnAQA-KEeGG9cEVh9B7ZA" name="Seuls les utilisateurs d'organisme de niveau 1 ou 2 peuvent gérer les droits" id="CS_ABANDON_02_12_001" statement="Seuls les utilisateurs d'organisme de niveau 1 ou 2 peuvent gérer les droits" rationale="">
          
          
          
          
        </requirements>
        <requirements xmi:id="_hqTecA-OEeGG9cEVh9B7ZA" name="Ecran de sélection d'utilisateur ou de profil" id="CS_ABANDON_02_12_002" statement="L'écran présente deux onglets permettant soit de faire une gestion des droits depuis les utilisateurs soit depuis les profils"/>
        <requirements xmi:id="_niHVQBazEeGgAtcpIUkjng" name="Création de profil générique" id="CS_ABANDON_02_12_040" statement="Il n’est possible que de créer des profils génériques" rationale="">
          
        </requirements>
        <requirements xmi:id="_4u-dUBRXEeGTEuEVVJ_8xw" name="Affichage de la liste des postes responsables" id="CS_ABANDON_02_12_031" statement="La liste des postes responsables est affichées dans un tableau avec pour chaque ligne trié par libellé du poste :&#xD;&#xA;Le libellé du poste&#xD;&#xA;Le nom et le prénom de l'utilisateur associé&#xD;&#xA;" rationale="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_ELt_oBUXEeGir4usBuBXIA" name="User Stories UC12" id="USCU12">
        <requirements xmi:id="_2twwQBUXEeGir4usBuBXIA" name="Créer un profil générique" id="CUC12_US01" statement="L'utilisateur veut pouvoir regrouper un ensemble de droit sur des fonctions pour les affecter à des utilisateurs.&#xD;&#xA;&#xD;&#xA;L’utilisateur est associé à un organisme de niveau 1 ou de niveau 2.&#xD;&#xA;Le système dispose de l’ensemble des fonctions de l’application.&#xD;&#xA;Le système dispose de l’ensemble de la hiérarchie d’organisme&#xD;&#xA;&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer les droits&quot;, puis « Gérer les profils » &#xD;&#xA;L’utilisateur clique sur « ajouter un profil ». Via la tâche « Créer / Modifier un profil » il saisi un type générique et un libellé. Il sélectionne ensuite des fonctions via la tâche « Associer des fonctions » .&#xD;&#xA;"/>
        <requirements xmi:id="_CyEPoBUYEeGir4usBuBXIA" name="Associer un profil à un utilisateur " id="CUC12_US02" statement="L'utilisateur veut pouvoir associer un profil à un utilisateur.&#xD;&#xA;&#xD;&#xA;L’utilisateur est associé à un organisme de niveau 1 ou de niveau 2.&#xD;&#xA;Le système dispose de l’ensemble des fonctions de l’application.&#xD;&#xA;Le système dispose de l’ensemble de la hiérarchie d’organisme.&#xD;&#xA;Le système dispose de l’ensemble des utilisateurs de l’application.&#xD;&#xA;&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer les droits&quot;, puis « Gérer les profils » &#xD;&#xA;L’utilisateur choisi un profil puis clique sur « Associer des utilisateur ». Via la tâche « Associer un profil à des utilisateurs » il choisi un utilisateur via la LOV utilisateur et enregistre.&#xD;&#xA;&#xD;&#xA;"/>
        <requirements xmi:id="_C6kEUBUYEeGir4usBuBXIA" name="Modifier un profil spécifique" id="CUC12_US03" statement="L'utilisateur veut pouvoir ajouter une fonction à un profil existant&#xD;&#xA;&#xD;&#xA;L’utilisateur est associé à un organisme de niveau 1 ou de niveau 2.&#xD;&#xA;Un profil a été créé pour l’organisme de l’utilisateur.&#xD;&#xA;Le système dispose de l’ensemble des fonctions de l’application.&#xD;&#xA;Le système dispose de l’ensemble de la hiérarchie d’organisme&#xD;&#xA;&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer les droits&quot;, puis « Gérer les profils ». L’utilisateur choisi un profil puis clique sur le bouton « Modifier » et via la tâche « Créer / Modifier un profil » il modifie le libellé du profil et ajoute une fonction."/>
        <requirements xmi:id="_DFI8oBUYEeGir4usBuBXIA" name="Supprimer un profil" id="CUC12_US04" statement="L'utilisateur veut pouvoir supprimer un profil existant.&#xD;&#xA;&#xD;&#xA;L’utilisateur est associé à un organisme de niveau 1 ou de niveau 2.&#xD;&#xA;Un profil a été créé pour l’organisme de l’utilisateur et est associé à des utilisateurs.&#xD;&#xA;Le système dispose de l’ensemble de la hiérarchie d’organisme.&#xD;&#xA;&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer les droits&quot;, puis « Gérer les profils ». L’utilisateur choisi un profil puis clique sur sur « Associer des utilisateurs ». Via la tâche « Associer un profil à des utilisateurs » il sélectionne tous les utilisateurs associés et supprime l’association. Ensuite il supprime le profil.&#xD;&#xA;"/>
        <requirements xmi:id="_YBT0kBUYEeGir4usBuBXIA" name="Créer un poste responsable" id="CUC12_US05" statement="L'utilisateur veut pouvoir créer un poste responsable et l’associer à un utilisateur.&#xD;&#xA;&#xD;&#xA;L’utilisateur est associé à un organisme de niveau 1 ou de niveau 2.&#xD;&#xA;Le système connaît l’ensemble des utilisateurs.&#xD;&#xA;&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer les droits&quot;, puis « Gérer les profils »"/>
        <requirements xmi:id="_fFZiYBUYEeGir4usBuBXIA" name="Modifier un poste responsable" id="CUC12_US06" statement="L'utilisateur veut pouvoir modifier l’utilisateur associé à un poste responsable&#xD;&#xA;&#xD;&#xA;L’utilisateur est associé à un organisme de niveau 1 ou de niveau 2.&#xD;&#xA;Le système connaît l’ensemble des utilisateurs.&#xD;&#xA;Un poste responsable est créé pour l’organisme de l’utilisateur.&#xD;&#xA;&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer les droits&quot;, puis « Gérer les profils » &#xD;&#xA;"/>
        <requirements xmi:id="_kvL1YBUYEeGir4usBuBXIA" name="Supprimer un poste responsable" id="CUC12_US07" statement="L'utilisateur veut pouvoir modifier l’utilisateur associé à un poste responsable&#xD;&#xA;&#xD;&#xA;L’utilisateur est associé à un organisme de niveau 1 ou de niveau 2.&#xD;&#xA;Le système connaît l’ensemble des utilisateurs.&#xD;&#xA;Un poste responsable est créé pour l’organisme de l’utilisateur&#xD;&#xA;&#xD;&#xA;Ce scénario débute dans la tâche &quot;Gérer les droits&quot;, puis « Gérer les profils » "/>
      </subCategories>
    </subCategories>
    <subCategories xmi:id="_wCWhoEW-EeGPzJLveCIEOA" name="Gérer les utilisateurs" id="CU13">
      <subCategories xmi:id="_z3oKEEW-EeGPzJLveCIEOA" id="CU13_01 - Authentifier un utilisateur">
        <requirements xmi:id="_bX6dcEW_EeGPzJLveCIEOA" name="Droits : Authentifier les utilisateurs" id="CS_RGF_02_13_100" statement="L'écran authentifier les utilisateurs ne nécessite pas de droit, il est accessible par tous">
          
        </requirements>
        <requirements xmi:id="_6m5B8EW_EeGPzJLveCIEOA" name="Le nom de l’utilisateur est affiché" id="CS_RGF_02_13_101" statement="Le nom de l’utilisateur est affiché pour information">
          
          
        </requirements>
        <requirements xmi:id="_Ae_dwEXAEeGPzJLveCIEOA" name="La saisie du mot de passe" id="CS_RGF_02_13_102" statement="La saisie du mot de passe est obligatoire" rationale="">
          
        </requirements>
        <requirements xmi:id="_HBjy4EXAEeGPzJLveCIEOA" name="Mot de passe masqué" id="CS_RGF_02_13_103" statement="Les caractères saisis dans la zone de mot de passe doivent être transformé en * à l'affichage">
          
        </requirements>
        <requirements xmi:id="_O9ZBAEXAEeGPzJLveCIEOA" name="Valider les paramètres d'authentification" id="CS_RGF_02_13_104" statement="Pour s'authentifier, l'utilisateur doit saisir un login et un mot de passe" rationale="">
          
        </requirements>
        <requirements xmi:id="_cB71YEXAEeGPzJLveCIEOA" name="Dévérrouillage de l'application" id="CS_RGF_02_13_105" statement="Si les informations d'authentification sont validées par le système SSO, et que l’utilisateur est le même que l’utilisateur précédent alors déverrouiller l'application en remettant l'accès au dernier écran visible avec les données affichées et saisies." rationale="">
          
        </requirements>
        <requirements xmi:id="_PSD-kEXCEeGPzJLveCIEOA" name="Non validation des données d'authentification" id="CS_RGF_02_13_106" statement="Si l'authentification a échouée, la fenêtre de saisie du login et mot de passe est proposée à nouveau à l'utilisateur, les valeurs précédemment saisies d'authentification sont effacées." rationale="">
          
        </requirements>
        <requirements xmi:id="_d7fGEEXCEeGPzJLveCIEOA" name="Afficher l'écran de saisie des informations de validation" id="CS_RGF_02_13_107" statement="L'écran d'authentification permet la saisie d'un loggin et d'un mot de passe. Les valeurs de ces deux champs sont vides par défaut.">
          
        </requirements>
        <requirements xmi:id="_VLaQsGa1EeGKMpH0JoMuDA" name="Auto Complétion du login" id="CS_RGF_02_13_120" statement="Le système propose une aide à la saisie du login par auto-complétion (liste de proposition de login existants dés les 4 premiers caractères saisis)" rationale="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_YW38EEXEEeGPzJLveCIEOA" name="Compléter le profil d'un utilisateur" id="CU13_02">
        <requirements xmi:id="_ngV1kEXEEeGPzJLveCIEOA" name="Charger les informations de l'utilisateur" id="CS_RGF_02_13_201" statement="Le système charge les informations de l'utilisateur sélectionné :&#xD;&#xA;Login, nom, prénom, email, grade,fonction,ville,quartier,entité,téléphone,service,fax,téléphone,téléphone portable, bâtiment,email internet,commentaire, téléphone FT, Fax FT." rationale="">
          
        </requirements>
        <requirements xmi:id="_lWpvwEXFEeGPzJLveCIEOA" name="Affichage des informations de l'utilisateur" id="CS_RGF_02_13_202" statement="Afficher les informations de l'utilisateurs non saisissables. Seules la saisie du numéro de portable et l'adresse email sont possibles." rationale="">
          
        </requirements>
        <requirements xmi:id="_8eKd4EXFEeGPzJLveCIEOA" name="Droits : Accessibilité à cette fonction" id="CS_RGF_02_13_200" statement="Seuls les utilisateurs associés à au moins un profil disposant de la fonction &quot;Compléter_les_informations_utilisateur&quot; peuvent accéder à cette fonction et consulter les profils des utilisateurs selon le périmètre géographique défini par le niveau associé à la fonction" rationale="">
          
        </requirements>
        <requirements xmi:id="_N46yoEXIEeGPzJLveCIEOA" name="Enregistrer les informations de l'utilisateur" id="CS_RGF_02_13_203" statement="Ajouter ou mettre à jour les informations téléphone portable et email internet pour l'utilisateur" rationale="">
          
        </requirements>
        <requirements xmi:id="_AKHM8EsxEeGi3u6K18qggQ" name="Droits : Accessiblité à la fonction restreinte à l'utilisateur" id="CS_RGF_02_13_205" statement="Tous les utilisateurs peuvent accéder à cette fonction pour gérer ses propres données" rationale="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_sajWsEXNEeGPzJLveCIEOA" name="Modifier la liste des profils associés à un utilisateur" id="CU13_03">
        <requirements xmi:id="_xXDF4EXNEeGPzJLveCIEOA" name="Droits : Droits associé à cette fonction" id="CS_RGF_02_13_300" statement="Seuls les utilisateurs associés à au moins un profil disposant de la fonction &quot;Compléter_les_informations_utilisateur&quot; peuvent accéder à cette fonction" rationale="">
          
        </requirements>
        <requirements xmi:id="_BHuEAEdoEeG6HoyujI0WsQ" name="Demande de confirmation" id="CS_RGF_02_13_302" statement="La suppression du profil de la liste des profils de l'utilisateur ne peut se faire sans confirmation de l'utilisateur" rationale="">
          
        </requirements>
        <requirements xmi:id="_Ow6dkEdoEeG6HoyujI0WsQ" name="LOV Profils générique" id="CS_RGF_02_13_301" statement="La LOV profil permet de sélectionner un profil générique. &#xD;&#xA;&#xD;&#xA;Tous les profils (non déjà rattaché à l'utilisateur) du même organisme ou d'un organisme de hiérarchie supérieur à celui de l'utilisateur &#xD;&#xA;&#xD;&#xA;Données : &#xD;&#xA;Libellé &#xD;&#xA;Libellé court&#xD;&#xA;Commentaire&#xD;&#xA;&#xD;&#xA;Champs de Recherche:&#xD;&#xA;Libellé&#xD;&#xA;Libellé court&#xD;&#xA;&#xD;&#xA;Tri par défaut sur le libellé" rationale="">
          
        </requirements>
        <requirements xmi:id="_k7gQcEdoEeG6HoyujI0WsQ" name="Enregistrer la liste des profils" id="CS_RGF_02_13_304" statement="Persister l'association de l'utilisateur et des profils" rationale="">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_Suo1UEXREeGPzJLveCIEOA" name="Consulter les profils associés à un utilisateur" id="CU13_04">
        <requirements xmi:id="_is8sIEXREeGPzJLveCIEOA" name="Droits : Droit pour accéder à cette fonction" id="CS_RGF_02_13_400" statement="Seuls les utilisateurs ayant au moins un profil ayant la fonction &quot;Consulter_les_profils_d_un_utilisateur&quot; peut accèder à cette fonction" rationale="">
          
        </requirements>
        <requirements xmi:id="_00I7AEXREeGPzJLveCIEOA" name="Charger la liste des profils génériques" id="CS_RGF_02_13_401" statement="Le système charge la liste des profils génériques qui ne sont pas associés à l'utilisateur :&#xD;&#xA;Libellé court&#xD;&#xA;Libellé &#xD;&#xA;Commentaire&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_875sUEXREeGPzJLveCIEOA" name="Charger la liste des profils de l'utilisateur" id="CS_RGF_02_13_402" statement="Le système charge la liste des profils génériques associés à l'utilisateur :&#xD;&#xA;Libellé &#xD;&#xA;Libellé court&#xD;&#xA;Commentaire. &#xD;&#xA;Avec pour chaque fonction :&#xD;&#xA;Nom de la Fonction &#xD;&#xA;Description&#xD;&#xA;Niveau de la fonction" rationale="">
          
        </requirements>
        <requirements xmi:id="_ilvbcEXSEeGPzJLveCIEOA" name="Charger la liste des profils spécifiques" id="CS_RGF_02_13_403" statement="Charger la liste des instances en cours de profils spécifiques associés directement à l'utilisateur ou via un poste responsable de l'utilisateur :&#xD;&#xA;Libellé&#xD;&#xA;Libellé court&#xD;&#xA;Commentaire&#xD;&#xA;Date de début&#xD;&#xA;Temporaire&#xD;&#xA;Avec pour chaque fonction :Nom, Type&#xD;&#xA;Avec pour chaque opération : Code et libellé de l'opération" rationale="">
          
        </requirements>
        <requirements xmi:id="_BPoV4EXTEeGPzJLveCIEOA" name="Charger la liste des postes responsables" id="CS_RGF_02_13_404" statement="Le système charge la liste des postes responsables en cours (sans date de fin) associés à l'utilisateur et les codes et libellés des opérations pour lesquels ils sont acteurs :&#xD;&#xA;Le nom du poste responsable&#xD;&#xA;Code et libellé de l'opération&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_bx56sEXTEeGPzJLveCIEOA" name="Afficher la liste des profils associés à l'utilisateur" id="CS_RGF_02_13_405" statement="Afficher la liste des profils :&#xD;&#xA;Libellé &#xD;&#xA;Libellé court&#xD;&#xA;Commentaire&#xD;&#xA;Pour chaque fonction :&#xD;&#xA;Nom&#xD;&#xA;Type&#xD;&#xA;Niveau (1 : Global, 2 : Régional, 3 : Local)" rationale="">
          
        </requirements>
        <requirements xmi:id="_8AWYIEXTEeGPzJLveCIEOA" name="Afficher la liste des profils spécifiques" id="CS_RGF_02_13_406" statement="Afficher la liste des profils spécifiques associés à l'utilisateur soit directement ou via un poste responsable :&#xD;&#xA;Libellé&#xD;&#xA;Libellé court&#xD;&#xA;Pour l'opération : Code et libellé" rationale="">
          
        </requirements>
        <requirements xmi:id="_NnAs0EXUEeGPzJLveCIEOA" name="Afficher la liste des postes responsables" id="CS_RGF_03_13_407" statement="Afficher la liste des postes responsables associés à l'utilisateur :&#xD;&#xA;Nom du poste responsable. &#xD;&#xA;Code et nom d'opérations utilisant le poste responsable">
          
        </requirements>
        <requirements xmi:id="_vf_LQEsrEeGi3u6K18qggQ" name="Détail du profil" id="CS_RGF_02_13_408" statement="Afficher le détail du profil :&#xD;&#xA;Commentaire&#xD;&#xA;Pour chaque fonction : Nom de la fonction" rationale="">
          
        </requirements>
        <requirements xmi:id="_XofN0EsvEeGi3u6K18qggQ" name="Afficher les informations de l'utilisateur" id="CS_RGF_02_13_409" statement="Afficher le login de l'utilisateur ainsi que son organisme de niveau 2 et son organisme de niveau 3" rationale="">
          
          
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_fRiR8EaREeGIB7OgXFna0A" name="Gérer les utilisateurs" id="CU13_05">
        <requirements xmi:id="_DmaZQEaSEeGIB7OgXFna0A" name="Charger la liste des utilisateurs" id="CS_RGF_02_13_501" statement="Charger la liste des utilisateurs (login, nom, prénom) dont les organismes correspondent au niveau associer à la fonction &quot;gérer_les_utilisateurs&quot; et charger la liste des organismes (Code, libellé, hiérarchie)." rationale="">
          
        </requirements>
        <requirements xmi:id="_eFs7gEaSEeGIB7OgXFna0A" name="Droits d'accès à la fonctionnalité" id="CS_RGF_02_13_500" statement="Pour accéder à cette fonctionnalité l'utilisateur doit posséder au moins un profils avec la fonction &quot;gerer_les_utilisateurs&quot;" rationale="">
          
        </requirements>
        <requirements xmi:id="_TijQYEaTEeGIB7OgXFna0A" name="Navigateur : Affichage de l'arborescence de travail" id="CS_RGF_02_13_502" statement="L'arborescence affichée est constituée de la liste des utilisateurs.&#xD;&#xA;Elle est organisée comme suit : &#xD;&#xA;Organisme (libellé abrégé) &#xD;&#xA;Utilisateur (login)&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_7Ty5cEaqEeGqJcKYRuuWQQ" name="Navigateur : Affiche des organismes" id="CS_RGF_02_13_503" statement="L'utilisateur peut choisir que la liste des utilisateurs soit affichée selon l'organisme d'appartenance des utilisateurs, par niveau :&#xD;&#xA;Si le niveau global est coché, un nœud supplémentaire composé du liba de l’organisme de niveau 1 est ajouté.&#xD;&#xA;Si le niveau régional est coché, un nœud supplémentaire composé du liba des organismes de niveau 2 est ajouté.&#xD;&#xA;Si le niveau local est coché, un nœud supplémentaire composé du liba des organismes de niveau 3 est ajouté.&#xD;&#xA;Par défaut, tous les niveaux sont affichés.&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_17QfsEd0EeG6HoyujI0WsQ" name="Navigateur : Filtre sur l'organisme de l'utilisateur" id="CS_RGF_02_13_504" statement="Via une case à cocher, l'utilisateur peut demander à filtrer la liste aux utilisateurs qui sont uniquement de son organisme et de ceux des organismes de niveau inférieurs qui lui sont rattaché (affichage par défaut).&#xD;&#xA;&#xD;&#xA;Sinon, la liste des utilisateurs est déterminée par le niveau associé à la fonction &quot;Gérer_les_utilisateurs&quot; au profil utilisé par l'utilisateur. &#xD;&#xA;(si le niveau est 1, tous les utilisateurs seront affichés)&#xD;&#xA;" rationale="">
          
        </requirements>
        <requirements xmi:id="_A77-4Ed1EeG6HoyujI0WsQ" name="Navigateur : Filtre par attribut utilisateur" id="CS_RGf_02_13_506" statement="L'utilisateur peut filtrer la liste selon un attribut utilisateur (nom, prénom, email, grade, login, fonction, ville, entité, téléphone, fax, service, email internet, bâtiment) et un critère saisi. L'attribut choisi devient le premier niveau de l'arborescence affiché" rationale="">
          
        </requirements>
        <requirements xmi:id="_QdtqsEd1EeG6HoyujI0WsQ" name="Navigateur : Affichage des barres d'outils" id="CS_RGF_02_13_507" statement="L'écran affiche une barre d'outils :&#xD;&#xA;- Utilisateur&#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_is7NIEd1EeG6HoyujI0WsQ" name="Navigateur : Barre outil utilisateur" id="CS_RGf_02_13_508" statement="La barre d'outils utilisateur propose les outils suivants :&#xD;&#xA;-&#x9;Gérer les profils&#xD;&#xA;-&#x9;Modifier &#xD;&#xA;">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_iEMm0EaREeGIB7OgXFna0A" name="Créer modifier un utilisateur" id="CU13_06">
        <requirements xmi:id="_0ASa4Ec_EeGDLacwIlvbjQ" name="Droit : Accessibilité de la fonctionnalité" id="CS_RGF_02_13_600" statement="Cette fonctionnalité système n’est pas gérée par les droits"/>
        <requirements xmi:id="_Hh5ZUEdAEeGDLacwIlvbjQ" name="Déterminer si l’utilisateur est connu" id="CS_RGF_02_13_601" statement="Un utilisateur est considéré comme nouvel utilisateur s’il n’existe aucun autre utilisateur ayant un login identique"/>
        <requirements xmi:id="_K_LYgEdAEeGDLacwIlvbjQ" name="Créer un utilisateur" id="CS_RGF_02_13_602" statement="L’utilisateur est persisté en base de données avec les informations suivantes fournies par l’annuaire SID:&#xD;&#xA;-&#x9;Login&#xD;&#xA;-&#x9;Nom&#xD;&#xA;-&#x9;Prénom&#xD;&#xA;-&#x9;Email&#xD;&#xA;-&#x9;Grade&#xD;&#xA;-&#x9;Fonction&#xD;&#xA;-&#x9;Ville&#xD;&#xA;-&#x9;Quartier&#xD;&#xA;-&#x9;Entité&#xD;&#xA;-&#x9;Téléphone PNIA&#xD;&#xA;-&#x9;Fax PNIA&#xD;&#xA;-&#x9;Service&#xD;&#xA;-&#x9;Bâtiment&#xD;&#xA;-&#x9;Commentaire&#xD;&#xA;-&#x9;Téléphone FT&#xD;&#xA;-&#x9;Fax FT&#xD;&#xA;"/>
        <requirements xmi:id="_O3cDgEdAEeGDLacwIlvbjQ" name="Associer l’utilisateur à un organisme" id="CS_RGF_02_13_603" statement="L’utilisateur est associé à l’organisme de rattachement (de niveau 3) fourni par l’annuaire (ou autre mécanisme)"/>
        <requirements xmi:id="_UMhR0EdAEeGDLacwIlvbjQ" name="Associer l’utilisateur à un profil par défaut" id="CS_RGF_02_13_604" statement="Si le profil par défaut du niveau 3 de l’organisme de l’utilisateur est coché « associer aux utilisateurs » l’associer à l’utilisateur&#xD;&#xA;Sinon, si le profil par défaut du niveau 2 de l’organisme de l’utilisateur est coché « associer aux utilisateurs » l’associer à l’utilisateur&#xD;&#xA;Sinon, associer le profil taggué « Profil par défaut » de niveau 1 à l’utilisateur&#xD;&#xA;"/>
        <requirements xmi:id="_IgwNQEdLEeGDLacwIlvbjQ" name="Identifier un utilisateur" id="CS_RGF_02_13_651" statement="Si un utilisateur possédant le même login est retrouvé dans la base, récupérer son identifiant."/>
        <requirements xmi:id="_Qnm_oEdLEeGDLacwIlvbjQ" name="Mettre à jour les informations d’un utilisateur" id="CS_RGF_02_13_652" statement="Les informations de l’utilisateur stockées en base sont remplacées par les données fournies par l’annuaire SID:&#xD;&#xA;-&#x9;Login&#xD;&#xA;-&#x9;Nom&#xD;&#xA;-&#x9;Prénom&#xD;&#xA;-&#x9;Email&#xD;&#xA;-&#x9;Grade&#xD;&#xA;-&#x9;Fonction&#xD;&#xA;-&#x9;Ville&#xD;&#xA;-&#x9;Quartier&#xD;&#xA;-&#x9;Entité&#xD;&#xA;-&#x9;Téléphone PNIA&#xD;&#xA;-&#x9;Fax PNIA&#xD;&#xA;-&#x9;Service&#xD;&#xA;-&#x9;Bâtiment&#xD;&#xA;-&#x9;Commentaire&#xD;&#xA;-&#x9;Téléphone FT&#xD;&#xA;-&#x9;Fax FT&#xD;&#xA;"/>
        <requirements xmi:id="_grKlYFFoEeGmiNbpPKKEBw" name="Associer l’utilisateur à une liste de travail par défaut " id="CS_RGF_02_13_605" statement="L’utilisateur est automatiquement associé à une liste de travail par défaut. Cette liste est crée pour l’utilisateur et nommée « Ma liste Perso »"/>
      </subCategories>
    </subCategories>
  </mainCategories>

<mainCategories xmi:id="_BUOa8XAMEeCNJtKfAFQMIw" name="Etudier la faisabilité" id="M01">
    <subCategories xmi:id="_BUOa8nAMEeCNJtKfAFQMIw" name="Organiser projet" id="CU01">
      <subCategories xmi:id="_BUObBXAMEeCNJtKfAFQMIw" name="Organiser le projet" id="T19">
        <requirements xmi:id="_mrn-0EN5EeGN3u1B5KdY6A" name="Droits : Accès à organiser projet" id="CS_RGF_01_01_000" statement="Seuls les utilisateurs ayant un profil avec la fonction « consulter_les_operations » dont le périmètre géographique s’étend au projet sélectionné peuvent accéder à cette tâche">
          
          
          
        </requirements>
        <requirements xmi:id="_BUObBnAMEeCNJtKfAFQMIw" name="[CS_RGF_01_015] - Etat opération - condition pour modifier les caractéristiques d'une opé " id="CS_RGF_01_015" statement="Pour pouvoir modifier les caractéristiques d'une opé (qui/ou, nature, identification), l'état de l'opération doit être différent de &quot;archivé&quot;&#xD;&#xA;" rationale="CS_FCT_010119001" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_BUObB3AMEeCNJtKfAFQMIw" name="[CS_RGF_01_018] - Récupérer les informations du projet" id="CS_RGF_01_018" statement="Consiste à récupérer les informations du projet, à savoir son code et son libellé.&#xD;&#xA;" rationale="CS_FCT010119004" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_BUObCXAMEeCNJtKfAFQMIw" name="[CS_RGF_01_020] - Projet - état du projet pour pouvoir créer des opérations" id="CS_RGF_01_020" statement="Seuls les projets dont l' état est &quot;actif&quot;, &quot;suspendu&quot;, &quot;provisoire incomplet&quot; ou &quot;provisoire complet&quot;  peuvent être organisés, c'est-à-dire que des opérations peuvent être créées" rationale="CS_FCT010119006" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_7tfAUEN_EeGN3u1B5KdY6A" name="Récupérer les informations de l’opération sélectionnée" id="CS_RGF_01_01_190" statement="Consiste à récupérer les informations de l’opération sélectionnée, à savoir son code, son libellé et son état.">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_BUOa_HAMEeCNJtKfAFQMIw" name="Creer des operations infrastructure" id="T25">
        <requirements xmi:id="_kqV0cEWmEeGpEKkOEP0AEw" name="Droits: accès à Créer des opérations d'infrastructure" id="CS_RGF_01_01_250" statement="Seuls les utilisateurs ayant un profil avec la fonction « creer_les_opérations » et dont le périmètre géographique s’étend à l’opération sélectionnée peuvent accéder à cette tâche" subtype="">
          
          
          
        </requirements>
        <requirements xmi:id="_BUOa_XAMEeCNJtKfAFQMIw" name="[CS_RGF_01_007] - Code opé  - génération" id="CS_RGF_01_007" statement="Le code opération est généré automatiquement selon la règle suivante: il est constitué du code projet(18 caractères) complété d'un tiret et d'un compteur d'opération (sur 3 caractères). Il est sur 22 caractères.&#xD;&#xA;&#xD;&#xA;En résumé: &#xD;&#xA;- le code projet sur 18 caractères  (cf CS_RGF_0_100 pour le détail)&#xD;&#xA;-  + &quot;_&quot;+&#xD;&#xA;- compteur par projet sur 3 caractères (complété par des 0 éventuellement)&#xD;&#xA;Ex. : USID_VRS_2011_0001_003 (ce qui signifie que l'opération 003 est la 3ème du projet USID_VRS_2011_0001)" rationale="CS_FCT_010125001" acceptanceCriteria="">
          
          
        </requirements>
        <requirements xmi:id="_BUOa_nAMEeCNJtKfAFQMIw" name="[CS_RGF_01_008] - L'identifiant de l'opération doit être calculé (max+1)" id="CS_RGF_01_008" statement="" rationale="CS_FCT_010125002&#xD;&#xA;" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_BUOa_3AMEeCNJtKfAFQMIw" name="[CS_RGF_01_009] - Constituer une liste d'opérations d'infrastructure" id="CS_RGF_01_009" statement="La liste est constituée de toutes les opérations d'infrastructure actives et dont l'état est différent de &quot;archivé&quot;. Les informations à présenter sont code + libellé + état, trié par code opération" rationale="" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_BUObAHAMEeCNJtKfAFQMIw" name="[CS_RGF_01_010] - Etat opération - calcul" id="CS_RGF_01_010" statement="L'état de l'opération est calculé dans 2 cas il vaut &quot;provisoire incomplet&quot; lorsque la saisie des champs de &quot;renseigner l'identification de l'opération&quot; est faite ET/OU &quot;renseigner la nature de l'opératioon&quot; OU &quot;renseigner le qui / ou&quot; sont saisis et corrects &#xD;&#xA;il vaut &quot;provisoire complet&quot; lorsque la saisie des champs de &quot;renseigner l'identification de l'opération&quot; ET &quot;renseigner la nature de l'opératioon&quot; ET &quot;renseigner le qui / ou&quot; sont saisis et corrects.&#xD;&#xA;L'état est modifiable par l'utilisateur lorsqu'il a une valeur différente de &quot;provisoire incomplet&quot;, &quot;provisoire complet&quot;." rationale="" acceptanceCriteria="">
          
          
        </requirements>
        <requirements xmi:id="_BUObAXAMEeCNJtKfAFQMIw" name="[CS_RGF_01_011] - Etat opération - passage à &quot;provisoire complet&quot;" id="CS_RGF_01_011" statement="L'état de l'opération est automatiquement renseigner à &quot;provisoire complet&quot;" rationale="CS_FCT_010125006" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_BUObAnAMEeCNJtKfAFQMIw" name="[CS_RGF_01_012] - Etat opération - passage à l'état &quot;provisoire incomplet&quot;" id="CS_RGF_01_012" statement="L'état de l'opération est renseigné automatiquement à &quot;provisoire incomplet&quot;" rationale="CS_FCT_010125006" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_BUObA3AMEeCNJtKfAFQMIw" name="[CS_RGF_01_013] - CRUD Opé - Historisation des modifications et saisies - exigence de traçabilité" id="CS_RGF_01_013" statement="Chaque saisie ainsi que modification dans l'application est historisée. La visu des données historisées sera faite dans le module administrateur" rationale="CS_FCT_010125007" acceptanceCriteria="">
          
          
        </requirements>
        <requirements xmi:id="_BUObBHAMEeCNJtKfAFQMIw" name="[CS_RGF_01_014] - Année création besoin opération - initialisation" id="CS_RGF_01_014" statement="L'année de création du besoin de l'opération est initialisée avec l'année de création du projet, mais peut être modifiée" rationale="ex CS_FCT_010125008" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_-iOPoLFBEeCQe4hWx-ILvw" name="[CS_RGF_01_071] - Contrôle des champs obligatoires – message d’erreur" id="CS_RGF_01_071" statement="Les champs obligatoires doivent être vérifiés. S’ils ne sont pas renseignés, les messages d’erreur suivant doivent être affichés :&#xD;&#xA;MSG_ERREUR sur organisme : « La saisie d’un organisme est obligatoire »&#xD;&#xA;MSG_ERREUR sur libellé : « La saisie d’un libellé est obligatoire »&#xD;&#xA;MSG_ERREUR sur année de création : « L’année de création de l’opération est invalide. Elle ne peut pas être antérieure à 2000 et postérieure à l'année courante + 100 » &#xD;&#xA;">
          
        </requirements>
        <requirements xmi:id="_zbNNALFDEeCQe4hWx-ILvw" name="[CS_RGF_01_072] – Validité de l’année de création" id="CS_RGF_01_072" statement="L’année saisie ne peut pas être antérieure à 2000 et postérieure à l'année courante + 100">
          
        </requirements>
      </subCategories>
      <subCategories xmi:id="_BUOa83AMEeCNJtKfAFQMIw" name="Renseigner l'identification de l'opération" id="T78">
        <requirements xmi:id="_sZbbgEghEeGNlJStOY93Ng" name="Droits: Accès à renseigner l'identification de l'opération" id="CS_RGF_01_01_780" statement="Seuls les utilisateurs ayant un profil avec la fonction « creer_les_opérations » ou la fonction « modifier_les_opérations » et dont le périmètre géographique s’étend à l’opération sélectionnée peuvent accéder à cette tâche.&#xD;&#xA;Les utilisateurs ayant un profil avec la fonction « consulter_les_opérations » et dont le périmètre géographique s’étend à l’opération sélectionnée peuvent accéder à cette tâche mais en consultation uniquement" status="">
          
          
        </requirements>
        <requirements xmi:id="_NNGR4Fi1EeG9PtOo88DCzQ" name="Droits: Accès à renseigner l'identification de l'opération – périmètre géographique" id="CS_RGF_01_01_784" statement="L’organisme SID pris en référence pour le calcul de périmètre géographique de l’opération est son entité SID (de niveau 3) en charge de la conduite des opérations, ou par défaut l’organisme SID (de niveau 2). ">
          
        </requirements>
        <requirements xmi:id="_BUOa9XAMEeCNJtKfAFQMIw" name="[CS_RGF_01_002] - Initier l'état de l'opération à &quot;provisoire incomplet&quot; par défaut" id="CS_RGF_01_002" statement="Le système doit initialiser l'opération avec un état positionné à &quot;provisoire incomplet&quot; par défaut." rationale="ex CS_FCT_010178003" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_BUOa9nAMEeCNJtKfAFQMIw" name="[CS_RGF_01_003] - Accèder à la liste des états d'opération" id="CS_RGF_01_003" statement="La liste des états d'opération est accessible à l'utilisateur uniquement lorsque que l'état est à &quot;en cours&quot;, avant la saisie n'est pas autorisée et le changement d'état est fait par programmation" rationale="" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_rshBIKh-EeCLJcBRoR43Gg" name="LOV Osiris" id="CS_RGF_01_302" statement="LOV Osiris&#xD;&#xA;- Sélection simple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Résultat :&#xD;&#xA;- trié par code OSIRIS croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- code&#xD;&#xA;- libellé&#xD;&#xA;&#xD;&#xA;Champs affichés :&#xD;&#xA;- Code&#xD;&#xA;- Libellé&#xD;&#xA;&#xD;&#xA;Champs retournés :&#xD;&#xA;code + &quot;-&quot; + lilbellé ">
          
        </requirements>
        <requirements xmi:id="_nwFLUKh_EeCLJcBRoR43Gg" name="LOV Code Plimat" id="CS_RGF_01_303" statement="LOV - Code Plimat&#xD;&#xA;- Sélection simple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Résultat :&#xD;&#xA;- trié par id plimat croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- identifiant&#xD;&#xA;- Organisme régional (code + lib)&#xD;&#xA;- Organisme local (code + lib)&#xD;&#xA;- Code plimat&#xD;&#xA;- Libellé&#xD;&#xA;Champs affichés :&#xD;&#xA;- Identifiant,&#xD;&#xA;- Libellé&#xD;&#xA;Champs retournés :&#xD;&#xA;- Id Plimat + &quot;-&quot; + libellé">
          
        </requirements>
        <requirements xmi:id="_G-J5sLkPEeCNR4w42RaY_g" name="LOV organisme SID conduisant l'opé" id="CS_RGF_01_304" statement="LOV - organisme SID conduisant l'opé&#xD;&#xA;- Sélection simple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Restriction:&#xD;&#xA;Présenter uniquement les organismes de niveau 2 du SID, c'est-à-dire les ESID, DCSID.&#xD;&#xA;Résultat :&#xD;&#xA;- trié par libellé abrégé croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- id organisme (RF_ORGANISME_ID)&#xD;&#xA;- libellé abrégé (RF_ORGANISME_LIBA)&#xD;&#xA;- Type organisme (RF_TORG_LIB)&#xD;&#xA;Champs retournés :&#xD;&#xA;- code organisme (RF_ORGANISME_COD)&#xD;&#xA;-  libellé abrégé de l'organisme (RF_ORGANISME_LIBA)">
          
        </requirements>
        <requirements xmi:id="_5JmSwLkQEeCNR4w42RaY_g" name="LOV entité SID en charge de la conduite d'opération" id="CS_RGF_01_305" statement="LOV - entité SID en charge de la conduite d'opération&#xD;&#xA;- Sélection simple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Restriction:&#xD;&#xA;Présenter uniquement les organismes de niveau 3 du SID, c’est-à-dire les USID et les pôles CO/MO et les DID.&#xD;&#xA;Résultat :&#xD;&#xA;- trié par libellé abrégé croissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- id organisme (RF_ORGANISME_ID)&#xD;&#xA;- libellé abrégé (RF_ORGANISME_LIBA)&#xD;&#xA;- Type organisme (RF_TORG_LIB)&#xD;&#xA;Champs retournés :&#xD;&#xA;- code organisme (RF_ORGANISME_COD)&#xD;&#xA;-  libellé abrégé de l'organisme (RF_ORGANISME_LIBA)" acceptanceCriteria="">
          
        </requirements>
        <requirements xmi:id="_VwFLALkREeCNR4w42RaY_g" name="Organisme SID conduisant l'opé - infobulle" id="CS_RGF_01_306" statement="Afficher dans l'infobulle de l'organisme SID conduisant l'opération le libellé long de l'organisme (RF_ORGANISME_LIB)">
          
        </requirements>
        <requirements xmi:id="_lPJhELkREeCNR4w42RaY_g" name="Entité SID en charge de la conduite d'opération  - infobulle" id="CS_RGF_01_307" statement="Afficher dans l'infobulle de l'&quot;entité SID en charge de la conduite d'opération&quot; le libellé long de l'organisme (RF_ORGANISME_LIB)">
          
        </requirements>
        <requirements xmi:id="_JQSvsEaOEeGe494aNO1lzQ" name="Identification - liste choix Type" id="CS_RGF_01_01_781" statement="Type d'opération (RF_???)&#xD;&#xA;- Sélection simple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Résultat :&#xD;&#xA;- trié par libellé croissant&#xD;&#xA;Champs affichés :&#xD;&#xA;- RF_???_LIB,&#xD;&#xA;Champs retournés :&#xD;&#xA;- RF_???_LIB,&#xD;&#xA;&#xD;&#xA;Valeurs ' ', 'A', 'B', 'C'">
          
        </requirements>
        <requirements xmi:id="_vSqTkEaOEeGe494aNO1lzQ" name="Identification - LOV éOTP" id="CS_RGF_01_01_782" statement="LOV - éOTP (PFE_RF_EOTP)&#xD;&#xA;- Sélection simple&#xD;&#xA;- actif (xtopsup=0)&#xD;&#xA;Résultat :&#xD;&#xA;- trié par code éOTP croissant &#xD;&#xA;Champs de recherche :&#xD;&#xA;- code  (RF_EOTP_COD)&#xD;&#xA;- libellé (RF_EOTP_LIB) &#xD;&#xA;Champs retournés :&#xD;&#xA;- code (RF_EOTP_COD)&#xD;&#xA;- libellé  (RF_EOTP_LIB)">
          
        </requirements>
        <requirements xmi:id="_jrifcEaOEeGe494aNO1lzQ" name="Identification - LOV TF" id="CS_RGF_01_01_783" statement="LOV - TF (PFE_RF_TFON)&#xD;&#xA;- Sélection simple PFE_RF_TFON&#xD;&#xA; - actif (xtopsup=0)&#xD;&#xA;Résultat :&#xD;&#xA;- trié par code TFcroissant&#xD;&#xA;Champs de recherche :&#xD;&#xA;- code  (RF_TFON_COD)&#xD;&#xA;- libellé (RF_TFON_LIB)&#xD;&#xA;Champs retournés :&#xD;&#xA;- code (RF_TFON_COD)&#xD;&#xA;- libellé  (RF_TFON_LIB)">
          
        </requirements>
        <requirements xmi:id="_WK6hcFi1EeG9PtOo88DCzQ" name="Organisme SID conduisant l'opé – initialisation" id="CS_RGF_01_01_785" statement="En création, l’organisme SID doit être initialisé avec l’organisme SID de niveau 2 de l’utilisateur, pour autant que l’utilisateur soit rattaché à un ESID ou DCSID, ou à un organisme en dépendant.">
          
        </requirements>
        <requirements xmi:id="_afn34Fi1EeG9PtOo88DCzQ" name="Entité SID en charge de la conduite d'opération  – initialisation" id="CS_RGF_01_01_786" statement="En création, l’entité SID en charge de la conduite d’opération doit être initialisée avec l’organisme SID de niveau 3 de l’utilisateur, pour autant que l’utilisateur soit rattaché à un USID, à un pôle CO/MO ou à un DID.">
          
        </requirements>
      </subCategories>
