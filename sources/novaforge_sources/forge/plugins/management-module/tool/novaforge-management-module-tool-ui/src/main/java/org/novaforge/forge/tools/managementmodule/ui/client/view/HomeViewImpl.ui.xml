<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res' type='org.novaforge.forge.tools.managementmodule.ui.client.ressources.ManagementModuleRessources' />
	
	<g:ScrollPanel alwaysShowScrollBars="false">
	<g:VerticalPanel spacing="5" height='100%' width='100%' verticalAlignment='ALIGN_TOP'>
	<g:HorizontalPanel width='100%' >
	 <g:cell width="50%">
	    <g:VerticalPanel spacing="5" width='100%' >
	    	<!-- PROJECT PLAN -->
			<g:Label ui:field='projectPlanTitle' addStyleNames="{res.css.zoneTitle}" />
	      	<g:VerticalPanel spacing="5" width='100%' addStyleNames="{res.css.zonePanel}">
	    	<g:VerticalPanel addStyleNames="{res.css.cellPanel}" >	
	    		<!-- <g:Button ui:field='projectPlanListB' /> -->	
           		<g:Anchor ui:field='projectPlanListLink' addStyleNames="{res.css.anchor}"/> 
           		<g:Anchor ui:field='projectPlanLotLink' addStyleNames="{res.css.anchor}"/> 
           		<g:Anchor ui:field='projectPlanIterationLink' addStyleNames="{res.css.anchor}"/> 
           		<g:Anchor ui:field='projectPlanMarkerLink' addStyleNames="{res.css.anchor}"/> 
           		<g:Anchor ui:field='projectPlanScopeLink' addStyleNames="{res.css.anchor}"/> 
           		<g:Anchor ui:field='projectPlanEstimationLink' addStyleNames="{res.css.anchor}"/>   
           		<g:Anchor ui:field='projectPlanChargePlanLink' addStyleNames="{res.css.anchor}"/>  	
	    	</g:VerticalPanel>
	    	</g:VerticalPanel>
	    </g:VerticalPanel>
	 </g:cell>
	 <g:cell>
	    <g:VerticalPanel spacing="5" width='100%' >
	    	<!-- ITERATION -->
			<g:Label ui:field='pilotageExecutionTitle' addStyleNames="{res.css.zoneTitle}" />
	      	<g:VerticalPanel  ui:field='pilotageExecutionPanel' spacing="5" width='100%' addStyleNames="{res.css.zonePanel}" >
	    	<g:VerticalPanel addStyleNames="{res.css.cellPanel}" >	
	    		<!--  <g:Button ui:field='iterationListB' /> -->
           		<g:Anchor ui:field='pilotageExecutionListLink' addStyleNames="{res.css.anchor}"/> 
           		<g:Anchor ui:field='pilotageExecutionScopeUnitDisciplineLink' addStyleNames="{res.css.anchor}"/> 
           		<g:Anchor ui:field='pilotageExecutionPreparationLink' addStyleNames="{res.css.anchor}"/> 
           		<g:Anchor ui:field='pilotageExecutionRefItLink' addStyleNames="{res.css.anchor}"/>  
           		<g:Anchor ui:field='pilotageExecutionReportItLink' addStyleNames="{res.css.anchor}"/> 
           		<g:Anchor ui:field='pilotageExecutionReportGlobalLink' addStyleNames="{res.css.anchor}"/>
           		<g:Anchor ui:field='pilotageExecutionBurnDownLink' addStyleNames="{res.css.anchor}"/> 
           		<g:Anchor ui:field='pilotageExecutionReportDrawLink' addStyleNames="{res.css.anchor}"/>
	    	</g:VerticalPanel>
	    	</g:VerticalPanel>
	    </g:VerticalPanel>
	 </g:cell>
	</g:HorizontalPanel>
	
	
	<g:HorizontalPanel width='100%' >
	 <g:cell width="50%">
	    <g:VerticalPanel spacing="5" width='100%' >
	    	<!-- ADMIN -->
			<g:Label ui:field='adminTitle' addStyleNames="{res.css.zoneTitle}" />
	      	<g:VerticalPanel spacing="5" width='100%' addStyleNames="{res.css.zonePanel}">
	    	<g:VerticalPanel addStyleNames="{res.css.cellPanel}" >
           		<g:Anchor ui:field='initialAdminLink' addStyleNames="{res.css.anchor}"/>
           		<g:Anchor ui:field='projectPlanAdminLink' addStyleNames="{res.css.anchor}"/>	    		
	    	</g:VerticalPanel>
	    	</g:VerticalPanel>
	    </g:VerticalPanel>
	 </g:cell>
	 <g:cell>
	    <g:VerticalPanel spacing="5" width='100%' >
<!-- 	    	MANTISARD -->
<!-- 			<g:Label ui:field='ardTitle' addStyleNames="{res.css.zoneTitle}" /> -->
<!-- 	      	<g:VerticalPanel spacing="5" width='100%' addStyleNames="{res.css.zonePanel}"> -->
<!-- 		    	<g:VerticalPanel addStyleNames="{res.css.cellPanel}" >	 -->
<!-- 	           		<g:Hyperlink ui:field='ardLink'/>  -->
<!-- 		    	</g:VerticalPanel> -->
<!-- 	    	</g:VerticalPanel> -->
	    </g:VerticalPanel>
	 </g:cell>
	 
	</g:HorizontalPanel>
	</g:VerticalPanel>
	</g:ScrollPanel>
</ui:UiBinder> 