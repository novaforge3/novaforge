<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
		>
	<ui:with field='res' type='org.novaforge.forge.tools.managementmodule.ui.client.ressources.ManagementModuleRessources' />
	<g:ScrollPanel alwaysShowScrollBars="false" ui:field="scrollPanel">
		<g:VerticalPanel spacing="5" height='100%' width='100%' verticalAlignment='ALIGN_TOP'>
			<g:Label ui:field='titleLabel' addStyleNames="{res.css.zoneTitle}" />
			<g:HorizontalPanel spacing="5" horizontalAlignment='ALIGN_LEFT'
				addStyleNames="{res.css.zonePanel}">
				<g:Grid ui:field="mainGrid">
					<g:row>
						<g:customCell>
							<g:Label ui:field="labelBugTrackerId" addStyleNames="{res.css.gridLabel}" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="bugTrackerIdTB" enabled="false"></g:TextBox>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="labelTitle" addStyleNames="{res.css.gridLabel}" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="titleTB" enabled="false"></g:TextBox>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="labelAssignedTo" addStyleNames="{res.css.gridLabel}" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="assignedToTB" enabled="false"></g:TextBox>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="labelStatus" addStyleNames="{res.css.gridLabel}" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="statusTB" enabled="false"></g:TextBox>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="labelSeverity" addStyleNames="{res.css.gridLabel}" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="severityTB" enabled="false"></g:TextBox>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="labelReporter" addStyleNames="{res.css.gridLabel}" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="reporterTB" enabled="false"></g:TextBox>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="labelPriority" addStyleNames="{res.css.gridLabel}" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="priorityTB" enabled="false"></g:TextBox>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="labelCategory" addStyleNames="{res.css.gridLabel}" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="categoryTB" enabled="false"></g:TextBox>
						</g:customCell>
					</g:row>
				</g:Grid>
			</g:HorizontalPanel>
			<!-- Buttons -->
			<g:HorizontalPanel ui:field="actionButtons" addStyleNames="{res.css.actionPanel}"
				horizontalAlignment='ALIGN_CENTER'>
				<g:HorizontalPanel width="200px" spacing="5" horizontalAlignment='ALIGN_CENTER'>
					<g:Button addStyleNames="{res.css.important}" ui:field="closeButton" />
				</g:HorizontalPanel>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:ScrollPanel>
</ui:UiBinder> 



