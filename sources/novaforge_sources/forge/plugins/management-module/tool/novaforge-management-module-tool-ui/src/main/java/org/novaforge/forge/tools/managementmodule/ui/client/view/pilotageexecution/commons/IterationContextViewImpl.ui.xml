<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res' type='org.novaforge.forge.tools.managementmodule.ui.client.ressources.ManagementModuleRessources' />
	<g:VerticalPanel width='100%' > 
		<g:HorizontalPanel horizontalAlignment='ALIGN_LEFT' width="100%">
		<g:HorizontalPanel spacing="5" addStyleNames="{res.css.gridRowPair}"
			horizontalAlignment='ALIGN_LEFT' width="100%">
			<g:HorizontalPanel spacing="5">
				<g:Label addStyleNames="{res.css.important}" ui:field="iterationL" />
				<g:Label addStyleNames="{res.css.labelCell}" ui:field="iteration" />
			</g:HorizontalPanel>
			<g:HorizontalPanel spacing="5">
				<g:Label addStyleNames="{res.css.important}" ui:field="lotL" />
				<g:Label addStyleNames="{res.css.labelCell}" ui:field="lot" />
			</g:HorizontalPanel>
			<g:HorizontalPanel spacing="5">
				<g:Label addStyleNames="{res.css.important}" ui:field="parentLotL" />
				<g:Label addStyleNames="{res.css.labelCell}" ui:field="parentLot" />
			</g:HorizontalPanel>
			<g:HorizontalPanel spacing="5">
				<g:Label addStyleNames="{res.css.important}" ui:field="startDateL" />
				<g:Label addStyleNames="{res.css.labelCell}" ui:field="startDate" />
			</g:HorizontalPanel>
			<g:HorizontalPanel spacing="5">
				<g:Label addStyleNames="{res.css.important}" ui:field="endDateL" />
				<g:Label addStyleNames="{res.css.labelCell}" ui:field="endDate" />
			</g:HorizontalPanel>
		</g:HorizontalPanel>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 