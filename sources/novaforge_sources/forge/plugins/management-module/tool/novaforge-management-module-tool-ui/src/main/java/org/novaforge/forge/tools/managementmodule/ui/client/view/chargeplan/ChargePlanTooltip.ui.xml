<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with field='res' type='org.novaforge.forge.tools.managementmodule.ui.client.ressources.ManagementModuleRessources' />
  <ui:with field='scoperes' type='org.novaforge.forge.tools.managementmodule.ui.client.properties.scope.ScopeMessage' />

  <g:VerticalPanel borderWidth="0" spacing="0" width='100%' height='100%' verticalAlignment='ALIGN_TOP'>
    <g:VerticalPanel spacing="0" width='100%'>
      <g:VerticalPanel spacing="5" width='100%' addStyleNames="{res.css.zonePanel}">
        <g:VerticalPanel spacing="5" width='100%'>
          <g:Grid addStyleNames="{res.css.zoneTable}">
            <g:row>
              <g:customCell>
                <g:Label styleName="{res.css.important}" ui:field="lotNamesLabel" />
              </g:customCell>
              <g:customCell>
                <g:Label ui:field="lotNames" width='100%'/>
              </g:customCell>
             </g:row>  
             <g:row>
              <g:customCell>
                <g:Label styleName="{res.css.important}" ui:field="subLotNamesLabel" />
              </g:customCell>
              <g:customCell>
                <g:Label ui:field="subLotNames" width='100%'/>
              </g:customCell>
             </g:row> 
             <g:row>
              <g:customCell>
                <g:Label styleName="{res.css.important}" ui:field="phaseNamesLabel" />
              </g:customCell>
              <g:customCell>
                <g:Label ui:field="phaseNames" width='100%'/>
              </g:customCell>
             </g:row>
			 <g:row>
              <g:customCell>
                <g:Label styleName="{res.css.important}" ui:field="iterationNamesLabel" />
              </g:customCell>
              <g:customCell>
                <g:Label ui:field="iterationNames" width='100%'/>
              </g:customCell>
             </g:row> 
          </g:Grid>
        </g:VerticalPanel>        
      </g:VerticalPanel>
    </g:VerticalPanel>
  </g:VerticalPanel>
</ui:UiBinder> 

